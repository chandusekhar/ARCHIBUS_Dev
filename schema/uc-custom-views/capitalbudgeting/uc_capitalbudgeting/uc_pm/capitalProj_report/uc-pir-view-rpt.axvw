<view version="2.0">
	<title>Capital Budget Report</title>
    <layout type="borderLayout" id="mainLayout">
       <west initialSize="255" split="true"/>
       <center autoScroll="true"/>
    </layout>
    
    <js file="uc-pir-view-rpt.js"/>
    
    <!-- display project list to the left of the view -->
    <panel type="view" id="projectProjList" file="uc-pir-request-proj-list-rpt.axvw" layout="mainLayout" region="west"  />
	<!--panel type="view" id="projectViewFormPanelpm"  file="uc-pir-view-main-rpt.axvw" layout="mainLayout" region="center" columns="4" /-->
	<panel type="view" id="projectViewSummaryFormPanel"  file="uc-pir-cust-summary.axvw" layout="mainLayout" columns="4" region="center">
	</panel>    
	
    <panel type="view" id="projectViewFormPanelcp"  file="uc-pir-cp-panels.axvw" layout="mainLayout" region="center" columns="4" >
	</panel>
    
    <panel type="view" id="projectViewGridPanelcpGrid"  file="uc-pir-cp-gridpanels.axvw" layout="mainLayout" region="center" columns="4" >
    </panel>

    <panel type="view" id="projectViewFormPanelen"  file="uc-pir-en-panels.axvw" layout="mainLayout" region="center" columns="4" showOnLoad = "false" hidden="true">
    </panel>
    
    <panel type="view" id="projectViewFormPanelca"  file="uc-pir-ca-panels.axvw" layout="mainLayout" region="center" columns="4" showOnLoad = "false" hidden="true">
    </panel>
    
    <panel type="view" id="projectViewFormPanelpag"  file="uc-pir-pag-panels.axvw" layout="mainLayout" region="center" columns="4" showOnLoad = "false" hidden="true">
    </panel>
</view>