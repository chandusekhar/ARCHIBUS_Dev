    <dataSource id="abRepmAddEditLeaseInABuildingCtry_ds">
        <table name="ctry" role="main"/>
        <field table="ctry" name="ctry_id"/>
        <restriction type="sql" sql="exists (select 1 from bl where bl.ctry_id = ctry.ctry_id and ${parameters['console']})"/>
        <parameter name="console" dataType="verbatim" value="1=1"/>
    </dataSource>

    <dataSource id="abRepmAddEditLeaseInABuildingCity_ds">
        <table name="city" role="main"/>
        <field table="city" name="city_id"/>
        <restriction type="sql" sql="exists (select 1 from bl where bl.city_id = city.city_id and ${parameters['console']})"/>
        <parameter name="console" dataType="verbatim" value="1=1"/>
    </dataSource>

    <dataSource id="abRepmAddEditLeaseInABuildingBl_ds">
        <table name="bl" role="main"/>
        <field table="bl" name="bl_id"/>
        <field table="bl" name="name"/>
        <restriction type="sql" sql="${parameters['console']}"/>
        <parameter name="console" dataType="verbatim" value="1=1"/>
    </dataSource>

    <dataSource id="abRepmAddEditLeaseInABuildingLs_ds1" distinct="true">
        <sql dialect="generic">
            select ls.ls_id ${sql.as} ls_parent_id, ls.tn_name, ls.uclass, ls.landlord_tenant, ls.bl_id from ls where use_as_template = 0 and ${parameters['consolels']}
        </sql>

        <table name="ls" role="main"/>
		<field table="ls" name="ls_parent_id"/>
        <field table="ls" name="tn_name"/>
		<field table="ls" name="uclass"/>
		<field table="ls" name="landlord_tenant"/>
        <parameter name="consolels" dataType="verbatim" value="1=1"/>
   </dataSource>

    <dataSource id="abRepmAddEditLeaseInABuildingSublease_ds">
        <table name="ls" role="main"/>
        <field table="ls" name="ls_id"/>
		<field table="ls" name="ls_id"/>
		<field table="ls" name="tn_name"/>
		<field table="ls" name="uclass"/>       
		<restriction type="parsed">
            <clause op="=" table="ls" name="use_as_template" value="0"/>
        </restriction>
        <restriction type="sql" sql="${parameters['consolels']}"/>
        <parameter name="consolels" dataType="verbatim" value="1=1"/>
    </dataSource>


