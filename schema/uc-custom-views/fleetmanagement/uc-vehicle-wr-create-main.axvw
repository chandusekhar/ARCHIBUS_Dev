<view version="2.0">
	<title>Create Vehicle Repair Request</title>
	<js file="uc-vehicle-wr-create-main.js"/>
	<js file="brg-common.js"/>
	<js file="brg-common-ui.js"/>
	<!-- confine tabs to the layout region -->
	<tabs workflow="enforced" id="wr_create_tabs" tabRefreshPolicy="refreshOnLoad">
		<!-- Tab page 1 (selected by default) -->
		<tab name="create_wr_info" selected="true" file="uc-vehicle-wr-create-1-Info.axvw" useFrame="false">
			<title translatable="true">My Information</title>
		</tab>

		<!-- Tab page 2 -->
		<tab name="create_wr_details" file="uc-vehicle-wr-create-2-Details.axvw" useFrame="false">
			<title translatable="true">Request Details</title>
		</tab>
		
		<!-- Tab page 3 -->
		<tab name="create_wr_documents" file="uc-vehicle-wr-create-3-docs.axvw" useFrame="false">
			<title translatable="true">Documents</title>
		</tab>
		
		<!-- Tab page 4 -->
		<tab name="create_wr_report" file="uc-vehicle-wr-create-4-Report.axvw" useFrame="false">
			<title translatable="true">Report</title>
		</tab>
	</tabs>
	
</view>