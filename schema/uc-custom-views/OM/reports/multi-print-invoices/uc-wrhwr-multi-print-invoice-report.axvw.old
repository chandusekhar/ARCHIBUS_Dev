		<field table="wrhwr" name="activity_log_id"/>
		<field table="wrhwr" name="assigned_cf" dataType="text"/>
		<field table="wrhwr" name="act_labor_hours"/>
		<field table="wrhwr" name="date_closed" />
		<field table="wrhwr" name="vehicle_id" />
