		<tab name="det_sub_req" file="uc-ds-wr-manager-details-5-subrequests.axvw" >
			<title translatable="true">Work Package</title>
		</tab>
		<tab name="det_pjha_entry" file="uc-ds-pjha-entry.axvw" useFrame="false">
			<title translatable="true">Enter PJHA</title>
		</tab>
