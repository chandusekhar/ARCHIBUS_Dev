<html lang="EN">
    <frameset rows="100,*" border="1" frameborder="1" framespacing="2">
        <frame scrolling="yes" name="console" title="Console Frame" frameborder="0" border="0" bordercolor="#1E4949" />
	    <frameset cols="35%,*" border="1" frameborder="1" framespacing="2">
	        <frame scrolling="yes" name="west" title="West Frame" frameborder="0" border="0" bordercolor="#1E4949" />
	        <frameset rows="250,*" border="1" frameborder="1" framespacing="2">
	            <frame scrolling="yes" name="north" title="North Frame" frameborder="0" border="0" />
	            <frame scrolling="yes" name="south" title="South Frame" frameborder="0" border="0" bordercolor="#1E4949" />
	        </frameset>
	    </frameset>
    </frameset>
</html>
