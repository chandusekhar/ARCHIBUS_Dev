<html lang="EN">
   <frameset cols="35%,*" border="1" frameborder="1" framespacing="2">
      <frameset rows="35%,*" border="1" frameborder="1" framespacing="2">
         <frame name="topSelectionFrame" title="Top Select Frame" scrolling="yes" frameborder="0" border="0" />
         <frame name="bottomSelectionFrame" title="Bottom Select Frame" scrolling="yes" frameborder="0" border="0" bordercolor="#1E4949" />
      </frameset>
      <frameset rows="50%,*" border="1" frameborder="1" framespacing="2">
         <frame name="topDetailsFrame" title="Top Details Frame" scrolling="yes" frameborder="0" border="0" />
         <frame name="bottomDetailsFrame" title="Bottom Details Frame" scrolling="yes" frameborder="0" border="0" bordercolor="#1E4949" />
      </frameset>
   </frameset>
</html>
