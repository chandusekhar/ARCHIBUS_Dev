<html lang="EN">
   <frameset rows="30%,*" border="1" frameborder="1" framespacing="2">
      <frame name="consoleFrame" title="Console Frame" scrolling="yes" frameborder="0" border="0"/>
      <frameset cols="30%,*" border="1" frameborder="1" framespacing="2">
         <frameset rows="40%,*" border="1" frameborder="1" framespacing="2">
           <frame name="topSelectionFrame" title="Top Select Frame" scrolling="yes" frameborder="0" border="0" />
           <frame name="bottomSelectionFrame" title="Bottom Select Frame" scrolling="yes" frameborder="0" border="0" bordercolor="#1E4949" />
         </frameset>
         <frame name="detailsFrame" scrolling="yes" frameborder="0" border="0" bordercolor="#1E4949" />
      </frameset>
   </frameset>
</html>
