<html lang="EN">
   <frameset cols="50%,*" border="1" frameborder="1" framespacing="2">
      <frameset rows="300,*,*" border="1" frameborder="1" framespacing="2">
         <frame scrolling="yes" name="topSelectionFrame" title="Top Select Frame" frameborder="0" border="0" />
         <frame scrolling="yes" name="bottomSelectionFrame" title="Bottom Select Frame" frameborder="0" border="0" bordercolor="#1E4949"/>
         <frame scrolling="yes" name="editFrame" title="Edit Frame" frameborder="0" border="0" bordercolor="#1E4949"/>
      </frameset>
      <frameset rows="300,*,*" border="1" frameborder="1" framespacing="2">
         <frame scrolling="yes" name="childFrame1" title="Content Frame 1" frameborder="0" border="0" />
         <frame scrolling="yes" name="childFrame2" title="Content Frame 2" frameborder="0" border="0" bordercolor="#1E4949"/>
         <frame scrolling="yes" name="childFrame3" title="Content Frame 3" frameborder="0" border="0" bordercolor="#1E4949"/>
      </frameset>
   </frameset>
</html>
