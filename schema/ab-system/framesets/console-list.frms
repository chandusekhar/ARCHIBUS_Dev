<html lang="EN">
   <frameset cols="400,*" border="1" frameborder="1" framespacing="2">
      <frameset rows="30%,*" border="1" frameborder="1" framespacing="2">
         <frame name="consoleFrame" title="Console Frame" scrolling="yes" frameborder="0" border="0" />
         <frame name="listFrame" title="List Frame" scrolling="yes" frameborder="0" border="0" bordercolor="#1E4949" viewFrame="true" />
      </frameset>
      <frame name="resultsFrame" title="Results Frame" scrolling="yes" frameborder="0" border="0" bordercolor="#1E4949" />
   </frameset>
  <formatting>
    <xsl file="ab-main-framesets.xsl"/>
  </formatting>
</html>
