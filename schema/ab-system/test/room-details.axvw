<?xml version="1.0" encoding="UTF-8"?>
<afmXmlView>
   <!--http://localhost:8080/archibus/schema/common/examples/room-details.axvw?handler=com.archibus.config.ActionHandlerDrawingItemSelected&rm.rm_id=102&bl.bl_id=HQ&fl.fl_id=17-->
   <preferences>
      <restrictionConsole show="false"/>
      <viewTitleBar show="false"/>
   </preferences>
   <title translatable="false">Room Details Report</title>
   <afmTableGroup type="report" format="table" showGrid="true">
      <title translatable="false">Highlight Room Report</title>
      <dataSource>
         <database>
            <tables>
               <table name="rm" role="main"/>
               <table name="rmstd" role="standard"/>
            </tables>
            <fields>
               <field name="bl_id" table="rm"/>
               <field name="fl_id" table="rm"/>
               <field name="rm_id" table="rm"/>
	        <field name="name" table="rm"/>
		<field name="rm_type" table="rm"/>
               <field name="area" table="rm"/>
               <field name="dv_id" table="rm"/>
	        <field name="dp_id" table="rm"/>
               <field name="description" table="rmstd"/>
            </fields>
         <sort><order><field name="bl_id" table="rm" ascending="true"/><field name="fl_id" table="rm" ascending="true"/><field name="rm_id" table="rm" ascending="true"/></order></sort></database>
      </dataSource>
   </afmTableGroup>
</afmXmlView>
