<afmXmlView>
  <afmTableGroup type="report" format = "table" defaultActions="true">
    <title translatable="false">Divisions and Departments by Floor</title>
    <dataSource>
      <database>
        <tables>
          <table name="rm" role="main" />
        </tables>
        <fields>
          <field name="bl_id" table="rm" />
          <field name="fl_id" table="rm" />
          <field name="dv_id" table="rm" />
          <field name="dp_id" table="rm" />
        </fields>
        <sort unique="true">
          <order>
            <field name="bl_id" table="rm" ascending="true"/>
            <field name="fl_id" table="rm" ascending="true"/>
            <field name="dv_id" table="rm" ascending="true"/>
            <field name="dp_id" table="rm" ascending="true"/>
          </order>
        </sort>
      </database>
    </dataSource>
  </afmTableGroup>
</afmXmlView>
