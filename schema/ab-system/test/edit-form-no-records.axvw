<afmXmlView>
   <afmTableGroup type="form" format="editForm" defaultActions="true">
      <title translatable="false">Request a Group Move</title>
      <dataSource>
         <database>
            <tables>
               <table name="project" role="main"/>
            </tables>
            <fields>
               <field name="project_id" table="project"/>
               <field name="description" table="project"/>
               <field name="dept_contact" table="project"/>
            </fields>
            <sort>
               <order>
                  <field name="project_id" table="project" ascending="true"/>
               </order>
            </sort>
            <restrictions>
               <restriction type="sql" sql="project_id is null"/>
            </restrictions>
         </database>
      </dataSource>
   </afmTableGroup>
</afmXmlView>
