<view version="2.0">
    <title>Copy Assessment Items to Projects</title>
    <js file="ab-condassess-copy-to-project.js"/>
    
    <tabs workflow="enforced" id="condassessCopyToProjectTabs">
        <tab name="condassessCopyToProjectPage1" file="ab-condassess-copy-to-project-page1.axvw" useFrame="true" selected="true">
            <title>Select Items</title>
        </tab>
        <tab name="condassessCopyToProjectPage2" file="ab-condassess-copy-to-project-page2.axvw" useFrame="true" selected="false">
            <title>Select Project</title>
        </tab>
        <tab name="condassessCopyToProjectPage3" file="ab-condassess-copy-to-project-page3.axvw" useFrame="true" selected="false">
            <title>Review Project Items</title>
        </tab>
     </tabs>  
</view>