    <message name="dateBeforeCurrent">You have entered a start date that has occurred in the past. Press OK to continue and save, or Cancel to return to form.</message>
    <message name="noWorkPackageDefinedPropose">You cannot propose a project without at least one work package assigned to that project. Use the Add New button from the Projects tree to add a new Work Package to your project.</message>
    <message name="noWorkPackageDefinedRequested">You cannot request a project without at least one work package assigned to that project. Use the Add New button from the Projects tree to add a new Work Package to your project.</message>
    <message name="titleDefaultAction" translatable="true">Project Location</message>

    <layout type="borderLayout" id="mainLayout">
        <north initialSize="90px" split="true"/>
        <center autoScroll="true"/>
    </layout>
    <layout id="nestedLayout_1" containingLayout="mainLayout" region="center">
        <west initialSize="30%" split="true"/>
        <center autoScroll="true"/>
    </layout>
    <title translatable="true">Edit My Projects</title>
    <js file="ab-project-edit.js"/>

    <dataSource id="projectEdit_consoleDs">
        <table name="project"/>
        <field table="project" name="project_id"/>
        <field table="project" name="status"/>
        <field table="project" name="site_id"/>
        <field table="project" name="bl_id"/>
        <field table="project" name="proj_mgr"/>
        <field table="project" name="project_type"/>
        <field table="project" name="program_id"/>
        <field table="project" name="priority"/>
        <sortField table="project" name="project_id" ascending="true"/>
        <restriction type="sql" sql="project.is_template = 0 AND project.requestor = ${sql.literal(user.employee.id)} AND project.status IN ('Created', 'Proposed', 'Requested')"/>
    </dataSource>

    <panel type="console" id="projectEdit_console" dataSource="projectEdit_consoleDs" columns="3" showOnLoad="true" layout="mainLayout" region="north">
        <title translatable="true">Filter</title>
        <action id="show">
            <title translatable="true">Show</title>
        </action>
        <action id="clear">
            <title>Clear</title>
        </action>
        <field table="project" name="site_id"/>
        <field table="project" name="project_type"/>
        <field name="project_id" table="project" op="=">
            <action id="projectEdit_console_selValProjectId">
                <title>...</title>
                <command type="selectValue" selectValueType="grid" fieldNames="project.project_id" selectFieldNames="project.project_id, project.project_name, project.project_type, project.requestor, project.summary" visibleFieldNames="project.project_id, project.project_name, project.project_type, project.requestor, project.summary" restriction="project.requestor = ${sql.literal(user.employee.id)} AND project.status IN ('Created', 'Proposed', 'Requested') AND project.is_template = 0">
                    <title>Project Code</title>
                </command>
            </action>
        </field>
        <field table="project" name="bl_id">
            <action id="projectEdit_console_selValBlId">
                <title>...</title>
                <command type="selectValue" showIndex="true" fieldNames="project.site_id, project.bl_id" selectFieldNames="bl.site_id, bl.bl_id" visibleFieldNames="bl.site_id, bl.bl_id, bl.name">
                    <title>Building Code</title>
                </command>
            </action>
        </field>
        <field table="project" name="program_id"/>
        <field>
            <title>Status</title>
            <html>
                <select class="inputField_box" id="status" name="status">
                    <option value="All" selected="1">
                        <span translatable="true"></span>
                    </option>
                    <option value="Created">
                        <span translatable="true">Created</span>
                    </option>
                    <option value="Proposed">
                        <span translatable="true">Proposed</span>
                    </option>
                    <option value="Requested">
                        <span translatable="true">Requested</span>
                    </option>
                </select>
            </html>
        </field>
    </panel>

    <dataSource id="projectEdit_projectTreeDs">
        <table name="project"/>
        <field table="project" name="project_id"/>
        <field table="project" name="project_name"/>
        <field table="project" name="status"/>
        <sortField table="project" name="project_id" ascending="true"/>
        <parameter name="project_restriction" dataType="verbatim" value="1=1"/>
        <restriction type="sql" sql="${parameters['project_restriction']}"/>
        <restriction type="sql" sql="project.is_template = 0 AND project.requestor = ${sql.literal(user.employee.id)} AND project.status IN ('Created', 'Proposed', 'Requested')"/>
    </dataSource>

    <dataSource id="projectEdit_projectTypeDs">
        <table name="projecttype" role="main"/>
        <field name="project_type"/>
        <field name="description"/>
    </dataSource>

    <dataSource id="projectEdit_workpkgTreeDs">
        <sql dialect="generic">
            SELECT
            work_pkgs.work_pkg_id ${sql.as} work_pkg_id,
            work_pkgs.project_id ${sql.as} project_id
            FROM work_pkgs
            UNION
            SELECT
            'WW99' ${sql.as} work_pkg_id,
            ${sql.isNull('activity_log.project_id', '\'WW99\'')} ${sql.as} project_id
            FROM activity_log
            WHERE activity_log.work_pkg_id IS NULL
        </sql>
        <table name="work_pkgs"/>
        <field table="work_pkgs" name="work_pkg_id"/>
        <sortField table="work_pkgs" name="work_pkg_id" ascending="true"/>
    </dataSource>

    <dataSource id="projectEdit_actionTreeDs">
        <sql dialect="generic">
            SELECT
            activity_log.activity_log_id ${sql.as} activity_log_id,
            activity_log.wbs_id ${sql.as} wbs_id,
            activity_log.action_title ${sql.as} action_title,
            ${sql.isNull('activity_log.work_pkg_id', '\'WW99\'')} ${sql.as} work_pkg_id,
            ${sql.isNull('activity_log.project_id', '\'WW99\'')} ${sql.as} project_id
            FROM activity_log
        </sql>
        <table name="activity_log"/>
        <field table="activity_log" name="activity_log_id"/>
        <field table="activity_log" name="wbs_id"/>
        <field table="activity_log" name="action_title"/>
        <sortField table="activity_log" name="wbs_id" ascending="true"/>
    </dataSource>

    <panel type="tree" id="projectEdit_projectTree" dataSource="projectEdit_projectTreeDs" layout="nestedLayout_1" region="west">
        <title translatable="true">My Created, Proposed, and Requested Projects</title>
        <action id="addNew">
            <title translatable="true">Add New</title>
        </action>
        <event type="onClickNode">
            <command type="callFunction" functionName="onClickTreeNode"/>
        </event>
        <panel type="tree" id="projectEdit_workpkgTree" dataSource="projectEdit_workpkgTreeDs">
            <event type="onClickNode">
                <command type="callFunction" functionName="onClickTreeNode"/>
            </event>
            <field table="work_pkgs" name="work_pkg_id"/>
        </panel>
        <panel type="tree" id="projectEdit_actionTree" dataSource="projectEdit_actionTreeDs">
            <event type="onClickNode">
                <command type="callFunction" functionName="onClickTreeNode"/>
            </event>
            <field table="activity_log" name="wbs_id"/>
            <field table="activity_log" name="action_title"/>
            <field table="activity_log" name="activity_log_id" hidden="true"/>
        </panel>
        <field table="project" name="project_id"/>
        <field table="project" name="project_name"/>
        <field table="project" name="status"/>
    </panel>

    <tabs id="projectEditTabs" workflow="enforced" layout="nestedLayout_1" region="center">
        <tab name="projectEdit_projectTab" selected="true">
            <title translatable="true">Project</title>
            <dataSource id="projectEdit_projectDs">
                <table name="project" role="main"/>
                <field name="project_id" table="project"/>
                <field name="project_name" table="project"/>
                <field name="project_type" table="project"/>
                <field name="status" table="project"/>
                <field name="program_id" table="project"/>
                <field name="dept_contact" table="project"/>
                <field name="phone_dept_contact" table="project"/>
                <field name="site_id" table="project"/>
                <field name="bl_id" table="project"/>
                <field name="dv_id" table="project"/>
                <field name="dp_id" table="project"/>
                <field name="area_affected" table="project"/>
                <field name="emp_affected" table="project"/>
                <field name="cost_budget" table="project"/>
                <field name="funding_probability" table="project"/>
                <field name="ac_id" table="project"/>
                <field name="date_start" table="project"/>
                <field name="date_end" table="project"/>
                <field name="days_per_week" table="project"/>
                <field name="duration_est" table="project"/>
                <field name="requestor" table="project"/>
                <field name="phone_req" table="project"/>
                <field name="date_created" table="project"/>
                <field name="doc" table="project"/>
                <field name="doc_risk_mgmt" table="project"/>
                <field name="doc_charter" table="project"/>
                <field name="doc_impl_plan" table="project"/>
                <field name="summary" table="project"/>
                <field name="description" table="project"/>
                <field name="scope" table="project"/>
                <field name="benefit" table="project"/>
                <field name="contact_id" table="project"/>
                <field name="is_template" table="project"/>
                <field name="project_quest" table="project"/>
                <field name="criticality" table="project"/>
                <field name="doc_scorecard" table="project"/>
            </dataSource>
            <panel type="form" id="projectEdit_projectForm" columns="2" dataSource="projectEdit_projectDs" showOnLoad="false">
                <title>Edit Project Profile</title>
                <action id="propose" enabled="${record['project.status'] == 'Created'}">
                    <title>Propose</title>
                </action>
                <action id="routeForApproval" enabled="${record['project.status'] == 'Requested'}">
                    <title>Route For Approval</title>
                </action>
                <action id="request" enabled="${record['project.status'] == 'Created' || record['project.status'] == 'Proposed'} ">
                    <title>Request</title>
                </action>
                <action id="delete" enabled="${record['project.status'] == 'Created'}">
                    <title>Withdraw</title>
                </action>
                <action id="save" enabled="${record['project.status'] != 'Requested-Routed'}">
                    <title>Save Changes</title>
                </action>
                <field name="project_id" table="project" readOnly="true"/>
                <field name="program_id" table="project"/>
                <field name="project_name" table="project"/>
                <field name="ac_id" table="project"/>
                <field name="project_type" table="project" required="true" readOnly="true"/>
                <field name="cost_budget" table="project"/>
                <field name="status" table="project" readOnly="true"/>
                <field name="funding_probability" table="project"/>
                <field/>
                <field/>
                <field name="summary" table="project" rowspan="2"/>
                <field name="date_start" table="project" required="true"/>
                <field name="date_end" table="project" required="true"/>
                <field name="description" table="project" rowspan="3"/>
                <field name="duration_est" table="project"/>
                <field name="days_per_week" table="project"/>
                <field/>
                <field name="scope" table="project" rowspan="3"/>
                <field name="site_id" table="project"/>
                <field name="bl_id" table="project">
                    <action id="projectEdit_selValBlId">
                        <title>...</title>
                        <command type="selectValue" showIndex="true" fieldNames="project.site_id, project.bl_id" selectFieldNames="bl.site_id, bl.bl_id" visibleFieldNames="bl.site_id, bl.bl_id, bl.name">
                            <title>Building Code</title>
                        </command>
                    </action>
                </field>
                <field name="dv_id" table="project"/>
                <field name="benefit" table="project" rowspan="3"/>
                <field name="dp_id" table="project"/>
                <field name="area_affected" table="project"/>
                <field name="emp_affected" table="project"/>
                <field/>
                <field/>
                <field name="doc" table="project"/>
                <field name="dept_contact" table="project" required="true">
                    <action id="projectEdit_selValDeptContact">
                        <title>...</title>
                        <command type="selectValue" fieldNames="project.dept_contact, project.phone_dept_contact" selectFieldNames="em.em_id, em.phone" visibleFieldNames="em.em_id,em.phone">
                            <title>Department Contact</title>
                        </command>
                    </action>
                </field>
                <field name="doc_risk_mgmt" table="project"/>
                <field name="phone_dept_contact" table="project">
                    <action id="projectEdit_selValPhoneDeptContact">
                        <title>...</title>
                        <command type="selectValue" fieldNames="project.dept_contact, project.phone_dept_contact" selectFieldNames="em.em_id, em.phone" visibleFieldNames="em.em_id,em.phone">
                            <title>Department Contact Phone</title>
                        </command>
                    </action>
                </field>
                <field name="doc_charter" table="project"/>
                <field name="requestor" table="project" readOnly="true"/>
                <field name="doc_impl_plan" table="project"/>
                <field name="phone_req" table="project">
                    <action id="projectEdit_selValPhoneReq">
                        <title>...</title>
                        <command type="selectValue" fieldNames="project.phone_req" selectFieldNames="em.phone" visibleFieldNames="em.em_id,em.phone">
                            <title>Requestor Phone</title>
                        </command>
                    </action>
                </field>
                <field name="criticality" table="project"/>
                <field name="date_created" table="project" readOnly="true"/>
                <field name="doc_scorecard" table="project"/>
                <field/>
                <field name="project_quest" table="project" hidden="true"/>
            </panel>
        </tab>
        <tab name="projectEdit_workpkgTab">
            <title translatable="true">Work Package</title>
            <dataSource id="projectEdit_workpkgDs">
                <table name="work_pkgs"/>
                <field name="work_pkg_id" table="work_pkgs"/>
                <field name="project_id" table="work_pkgs"/>
                <field name="status" table="work_pkgs"/>
                <field name="summary" table="work_pkgs"/>
                <field name="description" table="work_pkgs"/>
                <field name="proj_phase" table="work_pkgs"/>
                <field name="date_est_start" table="work_pkgs"/>
                <field name="date_est_end" table="work_pkgs"/>
                <field name="date_act_start" table="work_pkgs"/>
                <field name="date_act_end" table="work_pkgs"/>
                <field name="days_per_week" table="work_pkgs"/>
            </dataSource>
            <panel type="form" id="projectEdit_workpkgForm" dataSource="projectEdit_workpkgDs" showOnLoad="false">
                <title translatable="true">Edit Work Package</title>
                <action id="save">
                    <title translatable="true">Save</title>
                </action>
                <action id="deleteWorkpkg">
                    <title translatable="true">Delete</title>
                </action>
                <field name="work_pkg_id" table="work_pkgs" required="true"/>
                <field name="project_id" table="work_pkgs" required="true">
                    <action id="projectEdit_selValProjectId">
                        <title>...</title>
                        <command type="selectValue" fieldNames="work_pkgs.project_id" selectFieldNames="project.project_id" visibleFieldNames="project.project_id, project.project_name, project.status, project.summary" restriction="project.status NOT IN ('Requested-Rejected','Closed')"/>
                    </action>
                </field>
                <field name="proj_phase" table="work_pkgs"/>
                <field/>
                <field name="date_est_start" table="work_pkgs" required="true"/>
                <field name="date_est_end" table="work_pkgs" required="true"/>
                <field name="date_act_start" table="work_pkgs" hidden="true"/>
                <field name="date_act_end" table="work_pkgs" hidden="true"/>
                <field name="days_per_week" table="work_pkgs"/>
                <field/>
                <field name="summary" table="work_pkgs" style="height:2em"/>
                <field name="description" table="work_pkgs"/>
                <field name="status" table="work_pkgs" hidden="true" value="Created"/>
            </panel>
        </tab>
        <tab name="projectEdit_actionTab">
            <title translatable="true">Action</title>
            <dataSource id="projectEdit_actionDs">
                <table name="activity_log" role="main"/>
                <field name="activity_log_id" table="activity_log"/>
                <field name="project_id" table="activity_log"/>
                <field name="action_title" table="activity_log" required="true"/>
                <field name="proj_phase" table="activity_log"/>
                <field name="activity_type" table="activity_log" required="true"/>
                <field name="work_pkg_id" table="activity_log"/>
                <field name="site_id" table="activity_log"/>
                <field name="fl_id" table="activity_log"/>
                <field name="bl_id" table="activity_log"/>
                <field name="rm_id" table="activity_log"/>
                <field name="location" table="activity_log"/>
                <field name="prob_type" table="activity_log"/>
                <field name="tr_id" table="activity_log"/>
                <field name="wbs_id" table="activity_log"/>
                <field name="eq_id" table="activity_log"/>
                <field name="date_required" table="activity_log"/>
                <field name="date_planned_for" table="activity_log"/>
                <field name="date_scheduled" table="activity_log"/>
                <field name="duration_est_baseline" table="activity_log" required="true"/>
                <field name="hours_est_baseline" table="activity_log" required="true"/>
                <field name="duration" table="activity_log"/>
                <field name="hours_est_design" table="activity_log"/>
                <field name="cost_estimated" table="activity_log" required="true"/>
                <field name="cost_est_cap" table="activity_log" required="true"/>
                <field name="approved_by" table="activity_log"/>
                <field name="created_by" table="activity_log"/>
                <field name="doc" table="activity_log"/>
                <field name="date_requested" table="activity_log"/>
                <field name="description" table="activity_log"/>
            </dataSource>
            <panel type="form" id="projectEdit_actionForm" columns="1" dataSource="projectEdit_actionDs" showOnLoad="false">
                <title translatable="true">Edit Action</title>
                <action id="save">
                    <title translatable="true">Save</title>
                </action>
                <action id="delete">
                    <title translatable="true">Delete</title>
                </action>
                <field name="action_title" table="activity_log" required="true"/>
                <field name="activity_log_id" table="activity_log" readOnly="true" hidden="${record['activity_log.activity_log_id'] == ''}"/>
                <field name="activity_type" table="activity_log" required="true" value="PROJECT - TASK"/>
                <field name="project_id" table="activity_log" required="true">
                    <action id="projectEdit_selValActionProjectId">
                        <title>...</title>
                        <command type="selectValue" fieldNames="activity_log.project_id" selectFieldNames="project.project_id" visibleFieldNames="project.project_id, project.project_name, project.status, project.summary" restriction="project.status NOT IN ('Requested-Rejected','Closed')"/>
                    </action>
                </field>
                <field name="work_pkg_id" table="activity_log" required="false"/>
                <field name="proj_phase" table="activity_log"/>
                <field name="wbs_id" table="activity_log"/>
                <field/>
                <field name="site_id" table="activity_log" value="${user.employee.space.siteId}"/>
                <field name="bl_id" table="activity_log" value="${user.employee.space.buildingId}">
                    <action id="projectEdit_selValActivityBlId">
                        <title>...</title>
                        <command type="selectValue" showIndex="true" fieldNames="activity_log.site_id, activity_log.bl_id" selectFieldNames="bl.site_id, bl.bl_id" visibleFieldNames="bl.site_id, bl.bl_id, bl.name">
