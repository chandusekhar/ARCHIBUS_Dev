<view version="2.0">	
		<title>Commitment Progress Status Report</title>
    <js file="ab-proj-fcpm-cps.js"/>
    <js file="ab-proj-fcpm-cps-common.js"/>
    
	<tabs id="projFcpmCpsTabs" workflow="free">
        <tab name="projFcpmCpsProjs" file="ab-proj-fcpm-cps-projs.axvw" selected="true">
            <title>Select Project</title>
        </tab>
		<tab name="projFcpmCpsDash" file="ab-proj-fcpm-cps-rpt.axvw" >
            <title>Project</title>
        </tab>
        <tab name="projFcpmCpsPkg" hidden="false">
            <title>Work Package</title>
            <tabs id="projFcpmCpsPkgTabs" workflow="free">
                <tab name="projFcpmCpsPkgProf" file="ab-proj-fcpm-cps-pkg-prof.axvw" selected="true">
                    <title>Work Package Profile and Contract</title>
                </tab>
                <tab name="projFcpmCpsPkgChg" file="ab-proj-fcpm-cps-pkg-chg.axvw">
                    <title>Change Orders</title>
                </tab>
                <tab name="projFcpmCpsPkgInv" file="ab-proj-fcpm-cps-pkg-inv.axvw" >
                    <title>Invoices</title>
                </tab>
                <tab name="projFcpmCpsPkgAct" file="ab-proj-fcpm-cps-pkg-act.axvw">
                    <title>Actions and Projection</title>
                </tab>
            </tabs>
        </tab>
	</tabs>
</view>

