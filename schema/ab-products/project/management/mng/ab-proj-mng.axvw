<view version="2.0">	
		<title>Project Management Console</title>
    <js file="ab-query-parameters-utility.js"/>
    <js file="ab-proj-mng.js"/>
    <js file="ab-proj-mng-common.js"/>
    
	<tabs id="projMngTabs" workflow="free">
        <tab name="projMngProjs" file="ab-proj-mng-projs.axvw" selected="true">
            <title>Select Project</title>
        </tab>
				<tab name="projMngDash" file="ab-proj-mng-dash.axvw" >
            <title>Project Dashboard</title>
        </tab>
        <tab name="projMngInvs" file="ab-proj-mng-invs.axvw" >
            <title>Invoices</title>
        </tab>
        <tab name="projMngActs" file="ab-proj-mng-acts.axvw" >
            <title>Actions</title>
        </tab>
        <tab name="projMngGantt" file="ab-proj-mng-gantt.axvw" >
            <title>Gantt</title>
        </tab>
        <tab name="projMngPkg" hidden="true">
            <title>Work Package</title>
            <tabs id="projMngPkgTabs" workflow="free">
                <tab name="projMngPkgProf" file="ab-proj-mng-pkg-prof.axvw" selected="true">
                    <title>Work Package Profile and Contract</title>
                </tab>
                <tab name="projMngPkgChg" file="ab-proj-mng-pkg-chg.axvw">
                    <title>Change Orders</title>
                </tab>
                <tab name="projMngPkgInv" file="ab-proj-mng-pkg-inv.axvw" >
                    <title>Invoices</title>
                </tab>                
                <tab name="projMngPkgAct" file="ab-proj-mng-pkg-act.axvw">
                    <title>Actions</title>
                </tab>
            </tabs>
        </tab>
	</tabs>
</view>

