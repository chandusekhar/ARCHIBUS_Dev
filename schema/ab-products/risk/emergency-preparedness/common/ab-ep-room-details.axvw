<?xml version="1.0" encoding="UTF-8"?>
<afmXmlView>
   
   <preferences>
      <restrictionConsole show="false"/>
      <viewTitleBar show="false"/>
   </preferences>

   <afmTableGroup type="report" format="table" defaultActions="true">
      <dataSource>
         <database>
            <tables>
               <table name="rm" role="main"/>
            </tables>
            <sort>
               <order>
                  <field name="bl_id" table="rm" ascending="true"/>
                  <field name="fl_id" table="rm" ascending="true"/>
                  <field name="rm_id" table="rm" ascending="true"/>
               </order>
            </sort>
            <fields>
               <field name="rm_id" table="rm"/>
               <field name="recovery_status" table="rm"/>
               <field name="area" table="rm"/>
               <field name="rm_cat" table="rm"/>
               <field name="rm_type" table="rm"/>
               <field name="bl_id" table="rm"/>
               <field name="fl_id" table="rm"/>
            </fields>
         </database>
      </dataSource>

      <title translatable="true">Room Details</title>
      <afmReport printable="false"/>
   </afmTableGroup>

</afmXmlView>
