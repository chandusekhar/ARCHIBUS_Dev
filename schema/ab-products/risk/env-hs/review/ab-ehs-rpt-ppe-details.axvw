<view version="2.0">
    <title>Personal Protective Equipment Details</title>
    <js file="ab-ehs-rpt-ppe-details.js"/>
    
    <dataSource id="abEhsRptPPEDetails_ds">
        <table name="ehs_em_ppe_types"/>
        <field name="ppe_type_id"/>
        <field name="em_id"/>
        <field name="date_use"/>
        <field name="date_delivered"/>
        <field name="bl_id"/>
        <field name="fl_id"/>
        <field name="rm_id"/>
        <field name="doc"/>
    </dataSource>
    
    <panel type="form" id="abEhsRptPPEDetails_form" dataSource="abEhsRptPPEDetails_ds" showOnLoad="false">
        <title>Personal Protective Equipment Details</title>
        <field name="ppe_type_id" readOnly="true"/>
        <field name="em_id" readOnly="true"/>
        <field name="date_use" readOnly="true"/>
        <field name="date_delivered" readOnly="true"/>
        <field name="bl_id" readOnly="true"/>
        <field name="fl_id" readOnly="true"/>
        <field name="rm_id" readOnly="true"/>
        <field name="doc" readOnly="true"/>
    </panel>    
</view>