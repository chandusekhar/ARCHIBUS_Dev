<view version="2.0">
    <title>Employee Training Details</title>
    <js file="ab-ehs-rpt-training-details.js"/>
    
    <dataSource id="abEhsRptTrainingDetails_ds">
        <table name="ehs_training_results"/>
        <field name="training_id"/>
        <field name="em_id"/>
        <field name="date_actual"/>
        <field name="incident_id"/>
        <field name="status"/>
        <field name="description"/>
        <!--field name="doc"/-->
    </dataSource>
    
    <panel type="form" id="abEhsRptTrainingDetails_form" dataSource="abEhsRptTrainingDetails_ds" showOnLoad="false">
        <title>Employee Training</title>
        <field name="training_id" readOnly="true"/>
        <field name="em_id" readOnly="true"/>
        <field name="date_actual" readOnly="true"/>
        <field name="incident_id" readOnly="true"/>
        <field name="status" readOnly="true"/>
        <field name="description" readOnly="true"/>
        <!--field name="doc" readOnly="true"/-->
    </panel>
    
</view>