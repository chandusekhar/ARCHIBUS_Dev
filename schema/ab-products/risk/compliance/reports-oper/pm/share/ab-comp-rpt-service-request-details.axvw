<view version="2.0">
	<message name="pending" translatable="true">Pending</message>
	<message name="user" translatable="true">Step Responded By</message>
	<message name="on" translatable="true">On</message>
	<message name="step" translatable="true">Workflow Step</message>
	<message name="status" translatable="true">Status</message>
	<message name="stepstatus" translatable="true">Step Status After</message>
	<message name="comments" translatable="true">Comments</message>
	<message name="confirmCancel" translatable="true">Are you sure you want to cancel this request and remove it?</message>
	<message name="verificationNotAllowed" translatable="true">Verification not allowed</message>
	<message name="surveyNotAllowed" translatable="true">Satisfaction Survey not allowed</message>
	<message name="selectTitle" translatable="true">-select-</message>
	
	<js file="ab-comp-rpt-service-request-details.js" />
	<js file="ab-helpdesk-common-v2.js" />
	<js file="ab-questionnaire.js" />
	<js file="ab-ondemand-common.js" />
    <js file="ab-helpdesk-request-shared-para.js" />
	
	
	<message name="noDescription" translatable="true">Please enter a Description</message>
    <message name="exceedsMaxFieldSize">The questionnaire data exceeds the questionnaire results field size.  The data cannot be saved unless the field size is expanded.</message>
    <message name="auto_generated_response">A new action item has been automatically generated</message>
    <dataSource id="rmpctDS">
        <table name="wr" role="main"/>
        <table name="rmpct" role="standard"/>
        <field table="rmpct" name="activity_log_id"/>
        <field table="rmpct" name="pct_id"/>
        <field table="rmpct" name="bl_id"/>
        <field table="rmpct" name="fl_id"/>
        <field table="rmpct" name="rm_id"/>
        <field table="rmpct" name="date_start"/>

        <field table="rmpct" name="from_bl_id"/>
        <field table="rmpct" name="from_fl_id"/>
        <field table="rmpct" name="from_rm_id"/>
        <field table="rmpct" name="parent_pct_id"/>
        <field table="rmpct" name="date_end"/>
        <field table="rmpct" name="em_id"/>
        <field table="rmpct" name="status"/>
        <field table="rmpct" name="dv_id"/>
        <field table="rmpct" name="dp_id"/>
        <field table="rmpct" name="rm_cat"/>
        <field table="rmpct" name="rm_type"/>
        <field table="rmpct" name="primary_rm"/>
        <field table="rmpct" name="primary_em"/>
    </dataSource>
    
	<dataSource id="exPrgQuestionnaire_questionsDs">
		<table name="questions" role="main"/>
		<table name="questionnaire" role="standard"/>
	    <field name="questionnaire_id"/>
	    <field name="quest_name"/>
	    <field name="quest_text"/>
	    <field name="sort_order"/>
	    <field name="is_active"/>
	    <field name="format_type"/>
	    <field name="freeform_width"/>
	    <field name="lookup_table"/>
	    <field name="lookup_field"/>
	    <field name="action_response"/>
	    <field name="activity_type"/>
	    <field name="enum_list"/>
	    <field name="table_name" table="questionnaire"/>
	    <field name="field_name" table="questionnaire"/>
		<sortField name="sort_order" ascending="true"/>
	</dataSource>
				
	<dataSource id="viewDS">
		<table name="activity_log_hactivity_log" role="main" />

		<field name="created_by" table="activity_log_hactivity_log" />
		<field name="activity_log_id" table="activity_log_hactivity_log" />
		<field name="activity_type" table="activity_log_hactivity_log" />
		<field name="prob_type" table="activity_log_hactivity_log" />
		<field name="description" table="activity_log_hactivity_log" />
		<field name="requestor" table="activity_log_hactivity_log" />
		<field name="phone_requestor" table="activity_log_hactivity_log" />
		<field name="site_id" table="activity_log_hactivity_log" />
		<field name="bl_id" table="activity_log_hactivity_log" />
		<field name="fl_id" table="activity_log_hactivity_log" />
		<field name="rm_id" table="activity_log_hactivity_log" />
		<field name="location" table="activity_log_hactivity_log" />
		<field name="eq_id" table="activity_log_hactivity_log" />
		<field name="priority" table="activity_log_hactivity_log" />
		<field name="status" table="activity_log_hactivity_log" />
		<field name="doc1" table="activity_log_hactivity_log" />
		<field name="doc2" table="activity_log_hactivity_log" />
		<field name="doc3" table="activity_log_hactivity_log" />
		<field name="doc4" table="activity_log_hactivity_log" />
		<field name="act_quest" table="activity_log_hactivity_log" />
		<field name="dp_id" table="activity_log_hactivity_log" />
		<field name="dv_id" table="activity_log_hactivity_log" />
		<field name="cost_actual" table="activity_log_hactivity_log" />
		<field name="hours_actual" table="activity_log_hactivity_log" />
		<field name="cost_estimated" table="activity_log_hactivity_log" />
		<field name="hours_est_baseline" table="activity_log_hactivity_log" />
		<field name="comments" table="activity_log_hactivity_log" />
		<field name="satisfaction" table="activity_log_hactivity_log" />
		<field name="satisfaction_notes" table="activity_log_hactivity_log" />
		<field table="activity_log_hactivity_log" name="date_requested" />
		<field table="activity_log_hactivity_log" name="time_requested" />
		<field table="activity_log_hactivity_log" name="date_required" />
		<field table="activity_log_hactivity_log" name="time_required" />
		<field table="activity_log_hactivity_log" name="date_escalation_response" />
		<field table="activity_log_hactivity_log" name="time_escalation_response" />
		<field table="activity_log_hactivity_log" name="date_escalation_completion" />
		<field table="activity_log_hactivity_log" name="time_escalation_completion" />
		<field table="activity_log_hactivity_log" name="assigned_to" />
		<field table="activity_log_hactivity_log" name="vn_id" />
		<field table="activity_log_hactivity_log" name="supervisor" />
		<field table="activity_log_hactivity_log" name="manager" />
		<field table="activity_log_hactivity_log" name="work_team_id" />
		<field name="wr_id" table="activity_log_hactivity_log" readOnly="true" />
		<field name="wo_id" table="activity_log_hactivity_log" readOnly="true" />
	</dataSource>
	
	<dataSource id="DS_steps_log">
		<table name="helpdesk_step_log" role="main"/>
		<table name="afm_wf_steps" role="standard"/>
		<field name="step_log_id" table="helpdesk_step_log"/>
		<field name="user_name" table="helpdesk_step_log"/>
		<field name="em_id" table="helpdesk_step_log"/>
		<field name="vn_id" table="helpdesk_step_log"/>
		<field name="date_response" table="helpdesk_step_log"/>
		<field name="time_response" table="helpdesk_step_log"/>
		<field name="step" table="afm_wf_steps"/>
		<field name="status" table="afm_wf_steps"/>
		<field name="step_status_result" table="helpdesk_step_log"/>
		<field name="comments" table="helpdesk_step_log"/>
	</dataSource>

	<panel type="columnReport" columns="2" id="requestPanel" dataSource="viewDS">
		<title translatable="true">Service Request</title>
		<field name="activity_log_id" table="activity_log_hactivity_log"
			readOnly="true">
			<title translatable="true">Service Request ID</title>
		</field>
		<field name="created_by" table="activity_log_hactivity_log" readOnly="true" />
		
		<field name="requestor" table="activity_log_hactivity_log" readOnly="true" />
		<field name="phone_requestor" table="activity_log_hactivity_log" />
		
		<field table="activity_log_hactivity_log" name="date_requested" readOnly="true" />
		<field table="activity_log_hactivity_log" name="time_requested" readOnly="true" hidden="${record['activity_log_hactivity_log.time_requested'] == ''}" />
		
		<field name="wr_id" table="activity_log_hactivity_log" readOnly="true"
			hidden="${record['activity_log_hactivity_log.wr_id'] == ''}" />
		<field name="wo_id" table="activity_log_hactivity_log" readOnly="true"
			hidden="${record['activity_log_hactivity_log.wo_id'] == ''}" />
	</panel>

	<panel type="columnReport" columns="2" id="locationPanel" dataSource="viewDS">
		<title translatable="true">Work Location</title>
		<field name="site_id" table="activity_log_hactivity_log" readOnly="true" />
		<field name="bl_id" table="activity_log_hactivity_log" readOnly="true" />
		<field name="fl_id" table="activity_log_hactivity_log" readOnly="true" />
		<field name="rm_id" table="activity_log_hactivity_log" readOnly="true" />
		<field name="location" table="activity_log_hactivity_log" readOnly="true"
			hidden="${record['activity_log_hactivity_log.location'] == ''}" />
	</panel>
	
	<panel type="form" columns="2" id="equipmentPanel" dataSource="viewDS">
		<title translatable="true">Equipment</title>
		<field name="eq_id" table="activity_log_hactivity_log" readOnly="true" />
	</panel>

	<panel type="form" columns="2" id="descriptionPanel" dataSource="viewDS">
		<title translatable="true">Description</title>

		<field name="activity_type" table="activity_log_hactivity_log"
			readOnly="true">
			<title translatable="true">Request Type</title>
		</field>
	
		<field name="prob_type" table="activity_log_hactivity_log" readOnly="true" 
			hidden="${record['activity_log_hactivity_log.prob_type'] == ''}" />
		<field name="status" table="activity_log_hactivity_log" readOnly="true" />
		<field 	hidden="${record['activity_log_hactivity_log.prob_type'] == ''}" />
		
		<field name="description" table="activity_log_hactivity_log" readOnly="true"
			colspan="2" style="width:100%" />
		
		<field table="activity_log_hactivity_log" name="manager" readOnly="true"
			hidden="${record['activity_log_hactivity_log.manager'] == ''}" />
		<field table="activity_log_hactivity_log" name="assigned_to" readOnly="true"
			hidden="${record['activity_log_hactivity_log.assigned_to'] == ''}" />
			
		<field table="activity_log_hactivity_log" name="vn_id" readOnly="true"
			hidden="${record['activity_log_hactivity_log.vn_id'] == ''}" />
		<field table="activity_log_hactivity_log" name="work_team_id" readOnly="true"
			hidden="${record['activity_log_hactivity_log.work_team_id'] == ''}" />
			
		<field table="activity_log_hactivity_log" name="supervisor" readOnly="true"
			hidden="${record['activity_log_hactivity_log.supervisor'] == ''}" />
		<field name="act_quest" table="activity_log_hactivity_log" hidden="true" />
	</panel>
	
	<panel type="form" columns="2" id="priorityPanel" dataSource="viewDS">
		<title translatable="true">Priority</title>

		<field>
			<title translatable="true">Priority</title>
			<html>
				<span class="inputField" id="priority">
				</span>
			</html>
		</field>
		<field />
		
		<field table="activity_log_hactivity_log" name="date_required" readOnly="true"
			hidden="${record['activity_log_hactivity_log.date_required'] == ''}" />
		<field table="activity_log_hactivity_log" name="time_required" readOnly="true"
			hidden="${record['activity_log_hactivity_log.time_required'] == ''}" />
			
		<field table="activity_log_hactivity_log" name="date_escalation_response"
			readOnly="true"
			hidden="${record['activity_log_hactivity_log.date_escalation_response'] == ''}" />
		<field table="activity_log_hactivity_log" name="time_escalation_response"
			readOnly="true"
			hidden="${record['activity_log_hactivity_log.time_escalation_response'] == ''}" />
		
		<field table="activity_log_hactivity_log" name="date_escalation_completion"
			readOnly="true"
			hidden="${record['activity_log_hactivity_log.date_escalation_completion'] == ''}" />
		<field table="activity_log_hactivity_log" name="time_escalation_completion"
			readOnly="true"
			hidden="${record['activity_log_hactivity_log.time_escalation_completion'] == ''}" />
		
		<field name="priority" table="activity_log_hactivity_log" readOnly="true"
			hidden="true" />
	</panel>
	
	<panel type="grid" id="historyPanel" controlType="reportGrid" dataSource="DS_steps_log" collapsed="true" showOnLoad="false">
		<title translatable="true">History</title>
		<field name="step_log_id" table="helpdesk_step_log" hidden="true"/>
		<field name="user_name" table="helpdesk_step_log" hidden="true"/>
		<field name="em_id" table="helpdesk_step_log" hidden="true"/>
		<field name="vn_id" table="helpdesk_step_log">
			<title translatable="true">Step Responded By</title>
		</field>
		<field name="date_response" table="helpdesk_step_log">
			<title translatable="true">On</title>
		</field>
		<field name="time_response" table="helpdesk_step_log" hidden="true"/>
		<field name="step" table="afm_wf_steps">
			<title translatable="true">Workflow Step</title>
		</field>
		<field name="status" table="afm_wf_steps">
			<title translatable="true">Status</title>
		</field>
		<field name="step_status_result" table="helpdesk_step_log">
			<title translatable="true">Step Status After</title>
		</field>
		<field name="comments" table="helpdesk_step_log">
			<title translatable="true">Comments</title>
		</field>
      </panel>
	
	<panel type="form" columns="2" id="documentsPanel" dataSource="viewDS" collapsed="true">
		<title translatable="true">Documents</title>
		<field name="activity_log_id" table="activity_log_hactivity_log" hidden="true"/>
		
		<field name="doc1" table="activity_log_hactivity_log" required="false"
			readOnly="true" hidden="${record['activity_log_hactivity_log.doc1'] == ''}" />
		<field name="doc2" table="activity_log_hactivity_log" required="false"
			readOnly="true" hidden="${record['activity_log_hactivity_log.doc2'] == ''}" />
			
		<field name="doc3" table="activity_log_hactivity_log" required="false"
			readOnly="true" hidden="${record['activity_log_hactivity_log.doc3'] == ''}" />
		<field name="doc4" table="activity_log_hactivity_log" required="false"
			readOnly="true" hidden="${record['activity_log_hactivity_log.doc4'] == ''}" />
	</panel>


	<panel type="form" columns="2" id="costsPanel" dataSource="viewDS" collapsed="true">
		<title translatable="true">Hours and Costs</title>

		<field name="cost_estimated" table="activity_log_hactivity_log"
			readOnly="true"
			hidden="${record['activity_log_hactivity_log.cost_estimated'] == '0.00'}" />
		<field name="hours_est_baseline" table="activity_log_hactivity_log"
			readOnly="true"
			hidden="${record['activity_log_hactivity_log.hours_est_baseline'] == '0' }" />
		<field name="cost_actual" table="activity_log_hactivity_log" readOnly="true" />
		<field name="hours_actual" table="activity_log_hactivity_log" readOnly="true" />
		<field name="comments" table="activity_log_hactivity_log" colspan="2"
			style="width:100%;height:60px" showSelectValueAction="false"
			readOnly="true" />
	</panel>
	
	<panel type="form" columns="2" id="satisfactionPanel" dataSource="viewDS" collapsed="true">
		<title translatable="true">Satisfaction</title>
		<field name="satisfaction" table="activity_log_hactivity_log" readOnly="true" />
		<field name="satisfaction_notes" table="activity_log_hactivity_log"
			readOnly="true" />
	</panel>
</view>