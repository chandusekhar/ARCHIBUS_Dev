<view version="2.0">
    <title translatable="true">Manage Notification Templates</title>
    
    <tabs id="notifyTemplateTabs" workflow="free"  tabRefreshPolicy="refreshOnLoad">
        <tab name="selectTemplate" selected="true" file="ab-comp-notify-template-tab1.axvw">
            <title translatable="true">Select Notification Template</title>
        </tab>

        <tab name="editTemplate"  file="ab-comp-notify-template-form.axvw">
            <title translatable="true">Define Notification Template</title>
        </tab>

        <tab name="assignedProgram"  file="ab-comp-notify-template-tab3.axvw">
            <title translatable="true">Assigned Compliance Programs</title>
        </tab>

        <tab name="assignedRequirement"  file="ab-comp-notify-template-tab4.axvw">
            <title translatable="true">Assigned Compliance Requirements</title>
        </tab>

        <tab name="assignProgram"  file="ab-comp-select-program.axvw" hidden="true">
            <title translatable="true">Assign Compliance Programs</title>
        </tab>

        <tab name="assignRequirement"  file="ab-comp-select-requirement.axvw" hidden="true">
            <title translatable="true">Assign Compliance Requirements</title>
        </tab>
    </tabs>
	<js file="ab-comp-notify-template.js" />
