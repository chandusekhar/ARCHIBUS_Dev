<view version="2.0">
    <title translatable="true">Manage Document Library</title>

    <js file="ab-comp-library.js"/>
    <js file="ab-comp-common.js"/>

	<tabs workflow="free" id="compDocTabs" tabRefreshPolicy="refreshOnLoad">
        <event type="afterTabChange">
        </event>
		<tab name="selectDoc"
			file="ab-comp-library-select.axvw" selected="true">
			<title translatable="true">Select Document</title>
		</tab>
        <tab name="defineDoc" file="ab-comp-library-form.axvw" enabled="false" >
            <title translatable="true">Define Document</title>
        </tab>
	</tabs>
</view>  