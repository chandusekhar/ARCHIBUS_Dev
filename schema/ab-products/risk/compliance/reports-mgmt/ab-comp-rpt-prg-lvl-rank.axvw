<view version="2.0">

    <js file="ab-comp-rpt-mgmt-control.js"/>
    <js file="ab-comp-rpt-prg-lvl-rank.js"/>

    <message name="scoreBoardTitle" translatable="true">Compliance Programs Count by Regulation Rank and Compliance Level</message>
    <message name="chartTitle1" translatable="true">Chart: Compliance Programs Count by Compliance Level and Regulation Rank</message>
    <message name="chartTitle2" translatable="true">Chart: Compliance Programs Count by Regulation Rank and Compliance Level</message>
    <message name="firstGroupFieldTitle" translatable="true">Compliance Level</message>
    <message name="secondGroupFieldTitle" translatable="true">Regulation Rank</message>
    <message name="calcFieldTitle" translatable="true">Compliance Program Count</message>
    <message name="calcFieldTitle2" translatable="true">Requirements Count</message>
    
    <layout id="main">
        <north id="console" initialSize="200" split="true"/>
        <center id="tabs"/>
    </layout>

    <title translatable="true">Compliance Program Count by Compliance Level and Regulation Rank</title>

    <panel type="view" id="consoleView" file="ab-comp-rpt-mgmt-pro-console.axvw"
        layoutRegion="console"/>

    <tabs workflow="free" id="mianTabs" layoutRegion="tabs" tabRefreshPolicy="never">
        <tab name="chart2Tab" file="ab-comp-rpt-mgmt-chart.axvw" selected="true">
            <title translatable="true">Compliance Level by Regulation Rank Chart</title>
        </tab>
        
        <tab name="chart1Tab" file="ab-comp-rpt-mgmt-chart.axvw" >
            <title translatable="true">Regulation Rank by Compliance Level Chart</title>
        </tab>
        
        <tab name="scoreBoardTab" file="ab-comp-rpt-mgmt-score-board.axvw">
            <title translatable="true">Scoreboard</title>
        </tab>
    </tabs>
</view>  