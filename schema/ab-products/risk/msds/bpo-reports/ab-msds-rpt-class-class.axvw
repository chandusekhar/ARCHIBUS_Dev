<view version="2.0">
    <title translatable="true">Hazard Classification Classes List</title>

	<panel type="view" id="sharedDataSourceView" file="ab-msds-hazard-ds.axvw"/>

	<panel type="grid" id="abMsdsRptClassClassGrid" dataSource="msdsHazardClassDS"  showOnLoad="true">
        <title translatable="true">Classification Classes</title>
        <action id="doc">
            <title translatable="true">DOCX</title>
            <command type="exportPanel" outputType="docx" panelId="abMsdsRptClassClassGrid"/>
        </action>
        <action id="pdf">
            <title translatable="true">PDF</title>
            <command type="exportPanel" outputType="pdf" panelId="abMsdsRptClassClassGrid" show="false"/>
        </action>
        <action id="xls">
            <title translatable="true">XLS</title>
            <command type="exportPanel" outputType="xls" panelId="abMsdsRptClassClassGrid" viewName="ab-msds-rpt-class-class.axvw"/>
        </action>
        <action id="data">
            <title translatable="true">DATA</title>
            <command type="exportPanel" outputType="txfr" panelId="abMsdsRptClassClassGrid"/> 
        </action>
    </panel> 
   
</view>