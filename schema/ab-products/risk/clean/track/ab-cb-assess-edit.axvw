        <field name="hcm_loc_typ_id" readOnly="${((view.taskInfo.taskId == 'Manage My Hazard Abatement Items' &amp;&amp; !panel.newRecord) || view.taskInfo.taskId == 'Manage My Abatement Activity Items')}">
