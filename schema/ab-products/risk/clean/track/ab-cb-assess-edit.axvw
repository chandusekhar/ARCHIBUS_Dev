        <field name="hcm_harea_id" readOnly="${((view.taskInfo.taskId == 'Manage My Hazard Abatement Items' &amp;&amp; !panel.newRecord) || view.taskInfo.taskId == 'Manage My Abatement Activity Items' || view.taskInfo.taskId == 'Manage Hazard Assessment Items')}"/>
        <field name="location" readOnly="${((view.taskInfo.taskId == 'Manage My Hazard Abatement Items' &amp;&amp; !panel.newRecord) || view.taskInfo.taskId == 'Manage My Abatement Activity Items')}" showSelectValueAction="true">
