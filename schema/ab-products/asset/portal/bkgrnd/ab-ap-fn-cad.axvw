<view version="2.0">
	<designInfo type="report" pattern="ab-viewdef-report" isAlterable="true"/>
	<title translatable="true">Draw Furniture Standards</title>

	<dataSource id="abApFnCad_ds">
		<table name="fn" role="main"/>
		<table name="fnstd" role="standard"/>
		<field table="fn" name="bl_id"/>
		<field table="fn" name="fl_id"/>
		<field table="fn" name="rm_id"/>
		<field table="fn" name="dwgname"/>
		<field table="fn" name="fn_std"/>
		<field table="fnstd" name="symbol"/>
		<field table="fnstd" name="description"/>
		<field table="fn" name="fn_id" hidden="true"/>
	</dataSource>

	<panel type="grid" id="abApFnCad_detailsPanel" dataSource="abApFnCad_ds" multipleSelectionEnabled="false">
		<sortField table="fn" name="fn_std" ascending="true"/>
		<title translatable="true">Furniture Standards</title>
		<field table="fn" name="bl_id"/>
		<field table="fn" name="fl_id"/>
		<field table="fn" name="rm_id"/>
		<field table="fn" name="dwgname"/>
		<field table="fn" name="fn_std"/>
		<field table="fnstd" name="symbol"/>
		<field table="fnstd" name="description"/>
	</panel>

</view>
