<view version="2.0">
    <title translatable="true">Reconcile Enterprise Assets</title>
    <tabs id="abEamReconcileTabs" tabRefreshPolicy="none">
        <tab name="abEamReconcileTabs_conn_exec" file="afm-connector-exec.axvw" selected="true">
            <title translatable="true">Execute Connectors</title>
        </tab>
        <tab name="abEamReconcileTabs_conn_select" file="ab-eam-reconcile-select.axvw">
            <title translatable="true">Connector Selection</title>
        </tab>
        <tab name="abEamReconcileTabs_asset_added" file="ab-eam-reconcile-added.axvw" enabled="false">
            <title translatable="true">Assets Added on {date}</title>
        </tab>
        <tab name="abEamReconcileTabs_asset_deleted" file="ab-eam-reconcile-other.axvw" enabled="false">
            <title translatable="true">Assets Deleted on {date}</title>
        </tab>
        <tab name="abEamReconcileTabs_asset_updated" file="ab-eam-reconcile-other.axvw" enabled="false">
            <title translatable="true">Assets Updated on {date}</title>
        </tab>
    </tabs>
    <js file="ab-eam-reconcile-console.js"/>
</view>