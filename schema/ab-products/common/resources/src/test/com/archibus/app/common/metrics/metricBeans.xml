<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="metric" class="com.archibus.app.common.metrics.domain.Metric" scope="prototype">
    </bean>
    
    <bean id="granularity" class="com.archibus.app.common.metrics.domain.Granularity" scope="prototype">
    </bean>
    
    <bean id="granularityDef" class="com.archibus.app.common.metrics.domain.GranularityDef" scope="prototype">
    </bean>
    
    <bean id="trendValue" class="com.archibus.app.common.metrics.domain.MetricTrendValue" scope="prototype">
    </bean>
    
    <bean id="scorecard" class="com.archibus.app.common.metrics.domain.Scorecard" scope="prototype">
    </bean>
</beans>
