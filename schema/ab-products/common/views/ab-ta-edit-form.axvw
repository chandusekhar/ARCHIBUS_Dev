<view version="2.0">
    <js file="ab-ta-edit-form.js"/>
    
    <message name="confirmSaveOnChangeTab" translatable="true">Do you want to save your changes before leaving this page?</message>
    
    <title translatable="true">Add Furniture</title>
     
    <tabs workflow="free" id="abFurnitureForm_tabs" tabRefreshPolicy="refreshOnSelect">
    
        <tab name="abFurnitureForm_tabGeneral" selected="true" file="ab-ta-edit-form-general.axvw">
            <title translatable="true">General</title>
        </tab>

        <tab name="abFurnitureForm_tabLocation" file="ab-ta-edit-form-location.axvw">
             <title translatable="true">Location</title>
        </tab>
               
        <tab name="abFurnitureForm_tabCosts" file="ab-ta-edit-form-costs.axvw">
            <title translatable="true">Costs</title>
        </tab>
        
    </tabs>
    
    <panel type="view" id="helperView" file="ab-ta-edit-form-common.axvw"/>
</view>