<?xml version="1.0" encoding="UTF-8"?>
<afmXmlView>
   <preferences>
      <viewTitleBar show="false"/>
   </preferences>
   <title translatable="true">Work Request Information</title>
   <afmTableGroup type="report" format="column" defaultActions="false">
      <title translatable="true">Work Request Information</title>
      <dataSource>
         <database>
            <tables>
               <table name="wr" role="main"/>
            </tables>
            <fields>
               <field name="wr_id" table="wr"/>
               <field name="requestor" table="wr"/>
               <field name="phone" table="wr"/>
               <field name="prob_type" table="wr"/>
               <field name="eq_id" table="wr"/>
               <field name="bl_id" table="wr"/>
               <field name="fl_id" table="wr"/>
               <field name="rm_id" table="wr"/>
               <field name="location" table="wr"/>
               <field name="priority" table="wr"/>
               <field name="date_requested" table="wr"/>
               <field name="time_requested" table="wr"/>
               <field name="description" table="wr"/>
            </fields>
            <sort>
               <order>
                  <field name="wr_id" table="wr" ascending="true"/>
               </order>
            </sort>
         </database>
      </dataSource>
   </afmTableGroup>
   <formatting>
      <xsl file="ab-wr-request-response.xsl"/>
   </formatting>
</afmXmlView>
