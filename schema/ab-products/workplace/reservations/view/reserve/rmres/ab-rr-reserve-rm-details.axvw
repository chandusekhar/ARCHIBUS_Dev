<!-- Create details view of the reserved room -->
<view version="2.0">
    <dataSource id="ds_ab-rr-reserve-rm-details_form_reserve_rm" applyVpaRestrictions="false">
        <table name="reserve_rm" role="main"/>
        <table name="rm_config" role="standard"/>
        <table name="rm_arrange_type" role="standard"/>
        <table name="reserve" role="standard"/>
        
        <field table="reserve_rm" name="rmres_id"/>
        <field table="reserve_rm" name="res_id"/>
        <field table="reserve_rm" name="bl_id"/>
        <field table="reserve_rm" name="fl_id"/>
        <field table="reserve_rm" name="rm_id"/>
        <field table="reserve_rm" name="config_id"/>
        <field table="reserve_rm" name="rm_arrange_type_id"/>
        <field table="rm_config" name="config_name"/>
        <field table="rm_arrange_type" name="arrange_name"/>
        <field table="reserve_rm" name="date_start"/>
        <field table="reserve_rm" name="time_start"/>
        <field table="reserve_rm" name="time_end"/>
        <field table="reserve" name="reservation_name"/>
        <field table="reserve" name="user_requested_by"/>
        <field table="reserve" name="phone"/>
        <field table="reserve_rm" name="attendees_in_room"/>
        <field table="reserve_rm" name="comments"/>
    </dataSource>
    
    <panel id="reserveDetailsPanel" type="form" columns="2" dataSource="ds_ab-rr-reserve-rm-details_form_reserve_rm">
        <field table="reserve_rm" name="rmres_id" readOnly="true" required="false"/>
        <field table="reserve_rm" name="res_id" readOnly="true" required="false"/>
        <field table="reserve_rm" name="bl_id" readOnly="true" required="false"/>
        <field table="reserve_rm" name="fl_id" readOnly="true" required="false"/>
        <field table="reserve_rm" name="rm_id" readOnly="true" required="false"/>
        <field table="reserve_rm" name="config_id" readOnly="true" required="false"/>
        <field table="reserve_rm" name="rm_arrange_type_id" readOnly="true" required="false"/>
        <field table="rm_config" name="config_name" readOnly="true" required="false"/>
        <field table="rm_arrange_type" name="arrange_name" readOnly="true" required="false"/>
        <field table="reserve_rm" name="date_start" readOnly="true" required="false"/>
        <field table="reserve_rm" name="time_start" readOnly="true" required="false"/>
        <field table="reserve_rm" name="time_end" readOnly="true" required="false"/>
        <field table="reserve" name="reservation_name" readOnly="true" required="false"/>
        <field table="reserve" name="user_requested_by" readOnly="true" required="false"/>
        <field table="reserve" name="phone" readOnly="true" required="false"/>
        <field table="reserve_rm" name="attendees_in_room" readOnly="true" required="false"/>
        <field table="reserve_rm" name="comments" readOnly="true" required="false" colspan="2"/>
    </panel>
</view>