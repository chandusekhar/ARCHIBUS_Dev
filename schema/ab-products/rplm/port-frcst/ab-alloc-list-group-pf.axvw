        <sortField name="dp_id" table="gp" ascending="true"/>
        <sortField name="gp_id" table="gp" ascending="true"/>
        <title translatable="true">Groups</title>

        <action type="menu" id="addGroups_menu">
        		<title translatable="true">Add Group</title>
        		<action id="AddNew">
		            <title translatable="true">Add Group</title>
		            <command type="callFunction" functionName="allocListGroupController.groupGrid_onAddNew()"/>
		        </action>
		       	<action id="AddGroupsRm">
		       			<title translatable="true">Add Groups from Room Inventory</title>
		       			<command type="callFunction" functionName="allocListGroupController.groupGrid_onAddGroups('rm')"/>
		       	</action>
		       	<action id="AddGroupsEm">
		       			<title translatable="true">Add Groups from Employee Inventory</title>
		       			<command type="callFunction" functionName="allocListGroupController.groupGrid_onAddGroups('em')"/>
		       	</action>
        </action>
        
        <action id="SplitFloor">
