<!-- Tabbed view that implements the find-manage pattern.
	The first tab page contains a console that allows the user to filter
	the content of the second and third tab pages. -->

<view version="2.0">
	<title translatable="true">Find/Manage Wizard Example</title>

    <!-- The wizard workflow is free: the user can select any tab page at any time. -->
	<tabs workflow="free" id="exTabsWizardFind_tabs">
		<tab name="exTabsWizardFind_page1" selected="true" file="ab-ex-tabs-wizard-find-filter.axvw"
			imageName="ab-icon-proj-task.gif">
			<title translatable="true">Filter by Status</title>
		</tab>
		
		<tab name="exTabsWizardFind_page2" file="ab-ex-tabs-wizard-find-report.axvw"
			imageName="ab-icon-proj-depend.gif">
			<title translatable="true">Work Requests - Report</title>
		</tab>
		
		<tab name="exTabsWizardFind_page3" file="ab-ex-tabs-wizard-find-grouping.axvw"
			imageName="ab-icon-proj-depend.gif">
			<title translatable="true">Work Requests Grouped by Problem Type</title>
		</tab>
	</tabs>
</view>