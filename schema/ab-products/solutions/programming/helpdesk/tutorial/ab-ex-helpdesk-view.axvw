<view version="2.0">
    <title>View Problem Details</title>
    
    <dataSource>
        <table name="activity_log"/>
        <field name="activity_log_id"/>
        <field name="activity_type"/>
        <field name="action_title"/>
        <field name="location"/>
        <field name="date_requested"/>
        <field name="status"/>
        <field name="description"/>
    </dataSource>
    
    <panel type="columnReport" id="problemDetails"></panel>
</view>