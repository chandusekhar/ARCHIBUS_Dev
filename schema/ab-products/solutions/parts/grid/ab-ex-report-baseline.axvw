<!-- Baseline 16.2 report without any 16.3 features or customizations -->
<afmXmlView>
  	<title translatable="true">Baseline Report</title>
    <afmTableGroup type="report" format="table">
        <dataSource>
            <database>
                <tables>
                    <table name="project" role="main"/>
                </tables>
                <fields>
                    <field table="project" name="project_id"/>
                    <field table="project" name="status"/>
                    <field table="project" name="date_start"/>
                    <field table="project" name="date_end"/>
                </fields>
            </database>
        </dataSource>
    </afmTableGroup>
</afmXmlView>
