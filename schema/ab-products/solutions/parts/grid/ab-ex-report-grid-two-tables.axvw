
<view version="2.0">
  <title translatable="true">Grid Report</title>
  <!-- Use Grid format. -->

  <dataSource id="reportGridTwoTables_ds">
    <table name="project" role="main"/>
    <table name="projecttype" role="standard"/>
    <field table="project" name="project_id"/>
    <field table="project" name="status"/>
    <field table="project" name="date_start"/>
    <field table="project" name="date_end"/>
    <field table="project" name="description"/>
    <field table="projecttype" name="description"/>
  </dataSource>
  <panel type="grid" id="reportGridTwoTables_grid" dataSource="reportGridTwoTables_ds">
    <title translatable="true">Projects</title>
    <field table="project" name="project_id"/>
    <field table="project" name="status"/>
    <field table="project" name="date_start"/>
    <field table="project" name="date_end"/>
    <field table="project" name="description"/>
    <field table="projecttype" name="description"/>
  </panel>
</view>