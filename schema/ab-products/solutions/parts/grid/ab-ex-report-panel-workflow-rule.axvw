<view version="2.0">
  <title>Hello World Example</title>

  <panel type="columnReport" id="helloWorldPanel" dataSource="none">
    <title>Use the button on the right to invoke the Hello World rule:</title>
    
    <action>
      <title translatable="true">Invoke Hello World</title>
      
      <!-- Call HelloWorld rule and display the message returned by the rule -->
      <command type="workflowRule" ruleId="AbSolutionsLogicAddIns-HelloWorld-sayHello" 
               showMessageAsPopup="true"/>
    </action>
  </panel>
</view>

