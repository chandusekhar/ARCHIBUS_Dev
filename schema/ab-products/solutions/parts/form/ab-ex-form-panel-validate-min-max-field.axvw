<view version="2.0">
  <dataSource id="formvalidatefunding_probability_ds">
    <table name="project" role="main"/>
    <field table="project" name="project_id"/>
    <field table="project" name="requestor"/>
    <field table="project" name="funding_probability">
    	<title>Funding Probability (Minimum:0, Maximum:100)</title>
    </field>
     <field table="project" name="priority"/>
    
  </dataSource>

  <panel type="form" columns="1" id="formvalidatefunding_probability_form" dataSource="formvalidatefunding_probability_ds">
    <title translatable="true">Validate Minimum or Maximum values</title>
     <action id="save">
      <title translatable="true">Save</title>
      <command type="saveForm"/>
    </action>
   </panel>
</view>