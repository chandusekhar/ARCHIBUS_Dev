<?xml version="1.0" encoding="UTF-8"?>
<afmXmlView bShowAlterButtons="false">
	<preferences>
		<pdfButton show="true" style="portrait"/>
	</preferences>
	<title translatable="true">Review Building Performance</title>

	<fileCategory extension="frms" fileName="pkey.frms"/>

	<afmTableGroup type="form" frame="primaryKeyFrame" format="table" defaultActions="true">
		<title translatable="true">Buildings</title>
		
		<afmTableGroup type="report" frame="detailsFrame" format="column" defaultActions="true">
			<title translatable="true">Building Performance Summary</title>
			<dataSource>
				<database>
					<tables>
						<table name="bl" role="main"/>
					</tables>
					<fields>
						<field name="bl_id" table="bl"/>
						<field name="name" table="bl"/>
						<field name="address1" table="bl"/>
						<field name="address2" table="bl"/>
						<field name="site_id" table="bl"/>
						<field name="date_bl" table="bl"/>
						<field name="count_fl" table="bl"/>
						<field name="use1" table="bl"/>
						<field name="contact_name" table="bl"/>
						<field name="contact_phone" table="bl"/>
						<field name="count_occup" table="bl"/>
						<field name="count_max_occup" table="bl"/>
						<field name="cost_sqft" table="bl"/>
						<field name="area_gross_int" table="bl"/>
						<field name="area_gross_ext" table="bl"/>
						<field name="area_rentable" table="bl"/>
						<field name="area_usable" table="bl"/>
						<field name="area_ls_negotiated" table="bl"/>
						<field name="ratio_ru" table="bl"/>
						<field name="ratio_ur" table="bl"/>
						<field name="area_avg_em" table="bl"/>
					</fields>
					<sort><order><field name="bl_id" table="bl" ascending="true"/></order></sort>
				</database>
			</dataSource>
		</afmTableGroup>
		
		<dataSource>
			<database>
				<tables>
					<table name="bl" role="main"/>
				</tables>
				<fields>
					<field name="bl_id" table="bl"/>
					<field name="name" table="bl"/>
				</fields>
				<sort><order><field name="bl_id" table="bl" ascending="true"/></order></sort>
			</database>
		</dataSource>
	</afmTableGroup>
</afmXmlView>