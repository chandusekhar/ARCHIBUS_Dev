<?xml version="1.0" encoding="UTF-8"?>
<afmXmlView>
	<preferences>
		<restrictionConsole show="false"/>
		<viewTitleBar show="true"/>
		<pdfButton show="true" style="landscape"/>
	</preferences>
	
	<afmTableGroup type="report" format="table" showGrid="true">
		<title translatable="true">Equipment List Details</title>
		<dataSource>
			<database>
				<tables>
					<table name="eq" role="main"/>
				</tables>
				<fields>
					<field name="eq_id" table="eq" distinct="false"/>
					<field name="eq_std" table="eq" distinct="false"/>
					<field name="use1" table="eq" distinct="false"/>
					<field name="bl_id" table="eq" distinct="false"/>
					<field name="fl_id" table="eq" distinct="false"/>
					<field name="rm_id" table="eq" distinct="false"/>
					<field name="dv_id" table="eq" distinct="false"/>
					<field name="dp_id" table="eq" distinct="false"/>
					<field name="em_id" table="eq" distinct="false"/>
					<field name="condition" table="eq" distinct="false"/>	
				</fields>
				<sort><order><field name="eq_id" table="eq" ascending="true"/></order></sort>
			</database>	
		</dataSource>
	</afmTableGroup>
</afmXmlView>