<afmXmlView>
	<preferences>
		<viewTitleBar show="false" />
	</preferences>
	<title translatable="false">Room Information</title>
	<afmTableGroup type="report" format="column">
		<title translatable="false">Room Information</title>
		<dataSource>
			<database>
				<tables>
					<table name="rm" role="main" />
				</tables>
				<fields>
					<field name="bl_id" table="rm" />
					<field name="fl_id" table="rm" />
					<field name="rm_id" table="rm" />
					<field name="area" table="rm" />
				</fields>
				<sort>
					<order>
						<field name="bl_id" table="rm" ascending="true" />
						<field name="fl_id" table="rm" ascending="true" />
						<field name="rm_id" table="rm" ascending="true" />
					</order>
				</sort>
			</database>
		</dataSource>

	<afmTableGroup type="report" format="table">
		<title translatable="false">Employees</title>
		<dataSource>
			<database>
				<tables>
					<table name="em" role="main" />
				</tables>
				<fields>
					<field name="em_id" table="em" />
					<field name="phone" table="em" />
					<field name="email" table="em" />
				</fields>
				<sort><order><field name="em_id" table="em" ascending="true" /></order></sort>
			</database>
		</dataSource>
	</afmTableGroup>

	<afmTableGroup type="report" format="table">
		<title translatable="false">Equipment</title>
		<dataSource>
			<database>
				<tables>
					<table name="eq" role="main" />
				</tables>
				<fields>
					<field name="eq_id" table="eq" />
					<field name="eq_std" table="eq" />
					<field name="use1" table="eq" />
				</fields>
				<sort><order><field name="eq_id" table="eq" ascending="true" /></order></sort>
			</database>
		</dataSource>
	</afmTableGroup>

	</afmTableGroup>
</afmXmlView>
