<afmXmlView>
	<preferences>
		<viewTitleBar show="false" />
	</preferences>
	<afmTableGroup type="form" format="editForm">
		<dataSource>
			<database>
				<tables>
					<table name="property" role="main" />
				</tables>
				<fields>
					<field table="property" name="pr_id" />
					<field table="property" name="date_purchase" />
					<field table="property" name="date_book_val" />
					<field table="property" name="date_market_val" />
					<field table="property" name="value_assessed_prop_tax" />
					<field table="property" name="tax_rate_prop" />
					<field table="property" name="name" />
					<field table="property" name="cost_purchase" />
					<field table="property" name="value_book" />
					<field table="property" name="value_market" />
					<field table="property" name="value_assessed_school_tax" />
				</fields>
			</database>
		</dataSource>
		<panels>
			<panel type="form" columns="2" id="values">
				<fields>
					<field table="property" name="pr_id" readOnly="true" />
					<field table="property" name="name" readOnly="true" />
					<field table="property" name="date_purchase" readOnly="true" />
					<field table="property" name="cost_purchase" readOnly="true" />
					<field table="property" name="date_book_val" readOnly="true" />
					<field table="property" name="value_book" readOnly="true" />
					<field table="property" name="date_market_val" readOnly="true" />
					<field table="property" name="value_market" readOnly="true" />
					<field table="property" name="value_assessed_prop_tax" readOnly="true" />
					<field table="property" name="value_assessed_school_tax" readOnly="true" />
					<field table="property" name="tax_rate_prop" readOnly="true" />					
					<field />
				</fields>
			</panel>
		</panels>
	</afmTableGroup>
</afmXmlView>