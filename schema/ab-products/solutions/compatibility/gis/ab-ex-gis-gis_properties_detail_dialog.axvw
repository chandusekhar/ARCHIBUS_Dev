<afmXmlView>
    <preferences>
        <viewTitleBar show="false"/>
    </preferences>
    
    <afmTableGroup type="form" format="editForm">
        <dataSource>
            <database>
                <tables>
                    <table name="property" role="main"/>
                </tables>
                <fields>
                    <field table="property" name="pr_id"/>
                    <field table="property" name="street"/>
                    <field table="property" name="city_id"/>
                    <field table="property" name="state_id"/>
                    <field table="property" name="ctry_id"/>
                    <field table="property" name="value_market"/>
                    <field table="property" name="income_total"/>
                    <field table="property" name="area_bl_est_rentable"/>
                    <field table="property" name="cost_operating_total"/>
                </fields>
            </database>
        </dataSource>
        
        <panels>
            <!-- This panel uses custom Select Value actions -->
            <panel type="form" columns="2" id="pr_form">
      	        <title translatable="true">Selected Property Details</title>
                <fields>
                    <field table="property" name="pr_id" readOnly="true"/>
                    <field table="property" name="street" readOnly="true"/>
                    <field table="property" name="city_id" readOnly="true"/>
                    <field table="property" name="state_id" readOnly="true"/>
                    <field table="property" name="ctry_id" readOnly="true"/>
                    <field table="property" name="value_market" readOnly="true"/>
                    <field table="property" name="income_total" readOnly="true"/>
                    <field table="property" name="area_bl_est_rentable" readOnly="true"/>
                    <field table="property" name="cost_operating_total" readOnly="true"/>
		</fields>
	    </panel>
        </panels>
    </afmTableGroup>
</afmXmlView>
