<view version="2.0">
    <title translatable="true">Properties</title>
	
	<dataSource id="dsPropertyByCtry">
		<table name="property" role="main"/>
		
		<field name="pr_id"/>
		<field name="lat"/>
		<field name="lon"/>
		<field name="name"/>
		<field name="area_manual"/>
	</dataSource>
	

    
    <panel type="grid" id="panel_data" dataSource="dsPropertyByCtry">
        <title translatable="true">Property</title>
        
        <field name="pr_id" table="property"/>
		<field name="lat" table="property"/>
		<field name="lon" table="property"/>
		<field name="name" table="property"/>
		<field name="area_manual" table="property"/>
        
	</panel>                
</view>