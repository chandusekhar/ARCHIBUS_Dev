<view version="2.0">
	<title>Test View for WebService Examples</title>
	
	<panel type="console" id="consoleImportAllEmployees" dataSource="none">
	    <title>Example of Job calling EmployeeService WebService (exposed by an abstract ERP)</title>
	    
		<action id="importAllEmployees">
			<title>Import All Employees</title>
			<command type="startJob" ruleId="AbSolutionsViewExamples-EmployeesJob-importAllEmployees" messageId="importAllEmployees"/>
		</action>
	</panel>
	<panel type="console" id="consoleUploadDocument" dataSource="none">
	    <title>Example of Job uploading document from WebCentral to MS SharePoint server</title>
	    
		<action id="uploadDocument">
			<title>Upload Document</title>
			<command type="startJob" ruleId="AbSolutionsViewExamples-DocumentJob-uploadDocument" messageId="uploadDocument"/>
		</action>
	</panel>
</view>