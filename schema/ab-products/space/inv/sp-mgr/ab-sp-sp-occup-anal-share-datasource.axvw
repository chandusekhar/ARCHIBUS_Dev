											&gt;rm.cap_em   then '5 '${sql.concat}${parameters['excCap']}
									end)
								when ${parameters['isOccu']}='yes' AND ${parameters['dateType']}='r'
								then ( case when rmcat.occupiable=0 then '1 '${sql.concat}${parameters['nonOccup']}  
