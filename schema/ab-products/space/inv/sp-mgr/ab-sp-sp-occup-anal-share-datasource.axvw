						gp.area ${sql.as} cal_value
				from ( select * from gp ${sql.as} rmpct where ${sql.vpaRestriction} and rmpct.portfolio_scenario_id is null) ${sql.as} gp left outer join fl on fl.bl_id=gp.bl_id and fl.fl_id=gp.fl_id   
							left outer join bl on bl.bl_id=gp.bl_id
				where ${parameters['secondGroupOptionForGp']}  is not null  AND ${parameters['secondGroupOptionForGp']}!='-' 
