
<!-- This view fragment defines the Locations filter and grid. -->

<view version="2.0">
    <js file="ab-sp-console-locations.js"/> 
    <message name="addBuilding" translatable="true">Add Building</message>
    <message name="addFloor" translatable="true">Add Floor</message>
    <message name="editBl" translatable="true">Edit Building</message>
    <message name="editFloor" translatable="true">Edit Floor</message>
    <message name="teamFloorNotInLocationList" translatable="true">This floor plan cannot be displayed because it does not satisfy the filter. Please change or clear the filter and try again.</message>
    <message name="teamSchemaUnavailable" translatable="true">You must update your schema to include the tables and fields used to handle Team Space before continuing. To do this, add new tables 'rm_team', 'team_assoc', and 'team_properties'. Also add new fields to the 'team' table.</message>
    <message name="teamUnavailableInSpaceMode" translatable="true">The Teams tab is available only in occupancy mode.</message>
    <message name="roomStandardUnavailable" translatable="true">Room standard tab is only available in space mode.</message>
    <!-- Location filter and list. -->

    <dataSource id="locationFilterDS">
        <table name="rm"/>
		<!-- Space Utilization: To enable Space Utilization functionaliy:
										1) Add two dashes ("- -") after the word "Start" in the line below, and 
										2) Add "!- -" immediately after the "<" in the line below that currently reads < Space utilization - End. -->
		<!-- Space utilization - Start>
        <table name="bas_data_clean_num" role="standard"/>
		< Space utilization - End. -->
        <table name="em" role="standard"/>
        <field name="bl_id" table="rm"/>
        <field name="fl_id" table="rm"/>
        <field name="rm_id" table="rm"/>
        <field name="dv_id" table="rm"/>
        <field name="dp_id" table="rm"/>
        <field name="rm_type" table="rm"/>
        <field name="rm_cat" table="rm"/>
        <field name="em_id" table="em"/>
        <field name="date_last_surveyed" table="rm"/>
		<!-- Space Utilization: To enable Space Utilization functionaliy:
										1) Add two dashes ("- -") after the word "Start" in the line below, and 
										2) Add "!- -" immediately after the "<" in the line below that currently reads < Space utilization - End. -->
		<!-- Space utilization - Start>
        <field name="time_measured" table="bas_data_clean_num"/>
		< Space utilization - End. -->
    </dataSource>

    <dataSource id="locationsDS" applyVpaRestrictions="false">
        <sql dialect="generic">
          SELECT  * FROM (
			  SELECT  
				 wraped_rm.bl_id,
				 wraped_rm.fl_id,
				 wraped_rm.dwgname,
				 ( SELECT
					SUM(rm.area)
					FROM rm
					LEFT OUTER JOIN rmcat ON rm.rm_cat = rmcat.rm_cat
					WHERE ${parameters['commonParameters']}
					AND ${parameters['emRestriction']}
					AND ${parameters['teamRmRestriction']} 
					AND rm.dwgname IS NOT NULL
					AND (rm.rm_cat IS NULL OR EXISTS (SELECT 1 FROM rmcat WHERE
					rm.rm_cat= rmcat.rm_cat AND rmcat.used_in_calcs !='no_totals'))
					AND rm.bl_id = wraped_rm.bl_id AND rm.fl_id = wraped_rm.fl_id AND rm.dwgname = wraped_rm.dwgname 
					GROUP BY rm.bl_id, rm.fl_id, rm.dwgname
				 ) ${sql.as} total_area,
					 
				  (	SELECT  SUM(rm.cap_em)  
					FROM rm  
					LEFT OUTER JOIN rmcat ON rm.rm_cat = rmcat.rm_cat
					WHERE ${parameters['commonParameters']}
					AND ${parameters['emRestriction']}
					AND ${parameters['teamRmRestriction']} 
					AND rm.dwgname IS NOT NULL 
					AND rm.bl_id = wraped_rm.bl_id AND rm.fl_id = wraped_rm.fl_id AND rm.dwgname = wraped_rm.dwgname 
					GROUP BY rm.bl_id, rm.fl_id, rm.dwgname 
				  ) ${sql.as} cap_em,				 
				 wraped_rm.count_em,
				 wraped_rm.total_count, 
				 fl.area_gross_ext ${sql.as} area_gross_ext,
				 fl.area_gross_int ${sql.as} area_gross_int,
				 fl.area_rentable ${sql.as} area_rentable,
				 fl.area_usable ${sql.as} area_usable 
			  FROM 
					(	SELECT
							rm.bl_id ${sql.as} bl_id,
							rm.fl_id ${sql.as} fl_id,
							rm.dwgname ${sql.as} dwgname,
							COUNT(em.em_id) ${sql.as} count_em,
							COUNT(distinct rm.bl_id ${sql.concat} rm.fl_id ${sql.concat} rm.rm_id) ${sql.as} total_count
							FROM rm
							LEFT OUTER JOIN rmcat ON rm.rm_cat = rmcat.rm_cat
							LEFT OUTER JOIN em ON rm.rm_id = em.rm_id AND rm.fl_id = em.fl_id AND rm.bl_id = em.bl_id
							LEFT OUTER JOIN bl on rm.bl_id=bl.bl_id 
							WHERE ${sql.getVpaRestrictionForTable('bl')} and ${sql.vpaRestriction} 
							and ${parameters['commonParameters']} 
							AND ${parameters['emRestriction']} 
							AND ${parameters['teamRmRestriction']} 
							AND rm.dwgname IS NOT NULL 
							GROUP BY rm.bl_id, rm.fl_id, rm.dwgname
					)  ${sql.as}  wraped_rm LEFT OUTER JOIN fl ON wraped_rm.fl_id = fl.fl_id and wraped_rm.bl_id = fl.bl_id  
		  ) ${sql.as} wraped_rm_for_oracleDb  WHERE ${parameters['totalArea']} and ${parameters['totalTeamCap']} 
        </sql>

        <table name="rm" role="main"/>
        <table name="em" role="standard"/>

        <field table="rm" name="bl_id" dataType="text"/>
        <field table="rm" name="fl_id" dataType="text"/>
        <field table="rm" name="dwgname" dataType="text"/>
        <field table="rm" name="total_area" dataType="number" size="8" decimals="2" showTotals="true" numericFormat="area"/>
        <field table="rm" name="cap_em" dataType="integer" showTotals="true">
            <sql dialect="generic">cap_em</sql>
        </field>
        <field table="rm" name="count_em" dataType="integer" showTotals="true">
            <sql dialect="generic">count_em</sql>
        </field>
        <field table="rm" name="total_count" dataType="number" decimals="0" showTotals="true"/>
        <field table="rm" name="area_gross_ext" dataType="number" decimals="2" showTotals="true" numericFormat="area"/>
        <field table="rm" name="area_gross_int" dataType="number" decimals="2" showTotals="true" numericFormat="area"/>
        <field table="rm" name="area_rentable" dataType="number" decimals="2" showTotals="true" numericFormat="area"/>
        <field table="rm" name="area_usable" dataType="number" decimals="2" showTotals="true" numericFormat="area"/>
        
        <sortField table="rm" name="bl_id"/>
        <sortField table="rm" name="fl_id"/>
        <sortField table="rm" name="dwgname"/>

        <parameter name="commonParameters" dataType="verbatim" value="2=2"/>
        <parameter name="dv_id" dataType="verbatim" value="1=1"/>
        <parameter name="dp_id" dataType="verbatim" value="1=1"/>
        <parameter name="bl_id" dataType="verbatim" value="1=1"/>
        <parameter name="fl_id" dataType="verbatim" value="1=1"/>
        <parameter name="rm_id" dataType="verbatim" value="1=1"/>
        <parameter name="rm_cat" dataType="verbatim" value="1=1"/>
        <parameter name="rm_type" dataType="verbatim" value="1=1"/>
        <parameter name="occupancy" dataType="verbatim" value="1=1"/>
        <parameter name="emRestriction" dataType="verbatim" value="1=1"/>
        <parameter name="organizationUnassigned" dataType="verbatim" value="1=1"/>
        <parameter name="typeUnassigned" dataType="verbatim" value="1=1"/>
        <parameter name="totalArea" dataType="verbatim" value="1=1"/>
        <parameter name="excludedHotalableRm" dataType="verbatim" value="5=5"/>
        <parameter name="teamRmRestriction" dataType="verbatim" value="6=6"/>
        <parameter name="totalTeamCap" dataType="verbatim" value="7=7"/>
		<parameter name="selectDateStart" dataType="text" value="1900-01-01"/>
		<parameter name="selectDateEnd" dataType="text" value="2900-01-01"/>
    </dataSource>

	<dataSource id="locationsTeamDS" applyVpaRestrictions="false">
        <sql dialect="generic">
          SELECT  * FROM (
			  SELECT  
				 wraped_rm.bl_id,
				 wraped_rm.fl_id,
				 wraped_rm.dwgname,
				 floor_team.floorTeamOccupancy,
				 ( SELECT
					SUM(rm.area)
					FROM rm
					LEFT OUTER JOIN rmcat ON rm.rm_cat = rmcat.rm_cat
					WHERE ${parameters['commonParameters']}
					AND ${parameters['emRestriction']}
					AND ${parameters['teamRmRestriction']} 
					AND rm.dwgname IS NOT NULL
					AND (rm.rm_cat IS NULL OR EXISTS (SELECT 1 FROM rmcat WHERE
					rm.rm_cat= rmcat.rm_cat AND rmcat.used_in_calcs !='no_totals'))
					AND rm.bl_id = wraped_rm.bl_id AND rm.fl_id = wraped_rm.fl_id AND rm.dwgname = wraped_rm.dwgname 
					GROUP BY rm.bl_id, rm.fl_id, rm.dwgname
				 ) ${sql.as} total_area,
					 
				  (	SELECT  SUM(rm.cap_em)  
					FROM rm  
					LEFT OUTER JOIN rmcat ON rm.rm_cat = rmcat.rm_cat
					WHERE ${parameters['commonParameters']}
					AND ${parameters['emRestriction']}
					AND ${parameters['teamRmRestriction']} 
					AND rm.dwgname IS NOT NULL 
					AND rm.bl_id = wraped_rm.bl_id AND rm.fl_id = wraped_rm.fl_id AND rm.dwgname = wraped_rm.dwgname 
					GROUP BY rm.bl_id, rm.fl_id, rm.dwgname 
				  ) ${sql.as} cap_em,				 
				  (    ( SELECT  SUM(rm.cap_em)  
							FROM rm  
							LEFT OUTER JOIN rmcat ON rm.rm_cat = rmcat.rm_cat
							WHERE rm.dwgname IS NOT NULL 
							AND rm.bl_id = wraped_rm.bl_id AND rm.fl_id = wraped_rm.fl_id AND rm.dwgname = wraped_rm.dwgname 
							AND not exists ( select 1 from rm_team where rm.bl_id = rm_team.bl_id AND rm.fl_id = rm_team.fl_id AND rm.rm_id = rm_team.rm_id and ( rm_team.date_end is null or ${sql.yearMonthDayOf('rm_team.date_end')} &gt;= ${parameters['selectDateStart']}) and ${sql.yearMonthDayOf('rm_team.date_start')} &lt;= ${parameters['selectDateEnd']} ) 
							GROUP BY rm.bl_id, rm.fl_id, rm.dwgname ) 
						- 
						( select count(1) from em where em.bl_id=wraped_rm.bl_id and em.fl_id=wraped_rm.fl_id and em.rm_id is not null) 
				  ) ${sql.as} total_team_cap,				 
				 wraped_rm.count_em,
				 wraped_rm.total_count, 
				 fl.area_gross_ext ${sql.as} area_gross_ext,
				 fl.area_gross_int ${sql.as} area_gross_int,
				 fl.area_rentable ${sql.as} area_rentable,
				 fl.area_usable ${sql.as} area_usable 
			  FROM 
					(	SELECT
							rm.bl_id ${sql.as} bl_id,
							rm.fl_id ${sql.as} fl_id,
							rm.dwgname ${sql.as} dwgname,
							COUNT(em.em_id) ${sql.as} count_em,
							COUNT(distinct rm.bl_id ${sql.concat} rm.fl_id ${sql.concat} rm.rm_id) ${sql.as} total_count
							FROM rm
							LEFT OUTER JOIN rmcat ON rm.rm_cat = rmcat.rm_cat
							LEFT OUTER JOIN em ON rm.rm_id = em.rm_id AND rm.fl_id = em.fl_id AND rm.bl_id = em.bl_id
							LEFT OUTER JOIN bl on rm.bl_id=bl.bl_id 
							WHERE ${sql.getVpaRestrictionForTable('bl')} and ${sql.vpaRestriction} 
							and ${parameters['commonParameters']} 
							AND ${parameters['emRestriction']} 
							AND ${parameters['teamRmRestriction']} 
							AND rm.dwgname IS NOT NULL 
							GROUP BY rm.bl_id, rm.fl_id, rm.dwgname
					)  ${sql.as}  wraped_rm 
							LEFT OUTER JOIN fl ON wraped_rm.fl_id = fl.fl_id and wraped_rm.bl_id = fl.bl_id  
							LEFT OUTER JOIN  (
								select 
									wrap_rm_team.bl_id, wrap_rm_team.fl_id, wrap_rm_team.dwgname,  
									CAST( ROUND( sum(
																		CASE WHEN wrap_rm_team.teamCapcity IS NULL OR  wrap_rm_team.teamCapcity=0 THEN 0
																		ELSE wrap_rm_team.teamOccupancy*1.0*wrap_rm_team.teamFloorCapcity/wrap_rm_team.teamCapcity
																		END 
																	), 0 )  AS int ) ${sql.as} floorTeamOccupancy 
								from ( 
									select 
										rm_team.bl_id, rm_team.fl_id, rm.dwgname,  rm_team.team_id,  
										( select SUM(team.pct_time/100.00) from team 
																	where team.team_id=rm_team.team_id 
																	and (team.date_start is null or ${sql.yearMonthDayOf('team.date_start')} &lt;= ${parameters['asOfDate']})
																	and (team.date_end is null or ${sql.yearMonthDayOf('team.date_end')} &gt;= ${parameters['asOfDate']}) 
																	and team.em_id is not null 
										) ${sql.as} teamOccupancy,
										sum( rm.cap_em - ( select count( em.em_id) from em where em.bl_id=rm.bl_id and em.fl_id=rm.fl_id and em.rm_id=rm.rm_id ) ) ${sql.as} teamFloorCapcity, 
										( select sum(r.cap_em-(select count(1) from em where em.bl_id=r.bl_id and em.fl_id=r.fl_id and em.rm_id=r.rm_id) ) from rm_team ${sql.as} rt 
																left outer  join rm ${sql.as} r  on  rt.bl_id=r.bl_id and rt.fl_id=r.fl_id and rt.rm_id=r.rm_id 
																	where rt.team_id=rm_team.team_id 
																	and (rt.date_start is null or ${sql.yearMonthDayOf('rt.date_start')} &lt;= ${parameters['asOfDate']})
																	and (rt.date_end is null or ${sql.yearMonthDayOf('rt.date_end')} &gt;= ${parameters['asOfDate']}) 
										) ${sql.as} teamCapcity 
									from rm_team left outer join rm on  rm_team.bl_id=rm.bl_id and rm_team.fl_id=rm.fl_id and rm_team.rm_id=rm.rm_id 
									where  (rm_team.date_start is null or ${sql.yearMonthDayOf('rm_team.date_start')} &lt;= ${parameters['asOfDate']})
												and (rm_team.date_end is null or ${sql.yearMonthDayOf('rm_team.date_end')} &gt;= ${parameters['asOfDate']}) 
									group by rm_team.bl_id, rm_team.fl_id, rm.dwgname,  rm_team.team_id 
								) ${sql.as} wrap_rm_team 
								group by wrap_rm_team.bl_id, wrap_rm_team.fl_id, wrap_rm_team.dwgname 
							 ) ${sql.as} floor_team ON floor_team.bl_id=wraped_rm.bl_id and wraped_rm.fl_id = floor_team.fl_id and wraped_rm.dwgname = floor_team.dwgname  
		  ) ${sql.as} wraped_rm_for_oracleDb  WHERE ${parameters['totalArea']} and ${parameters['totalTeamCap']} 
        </sql>

          <sql dialect="sqlserver">
          SELECT  * FROM (
			  SELECT  
				 wraped_rm.bl_id,
				 wraped_rm.fl_id,
				 wraped_rm.dwgname,
				 ( SELECT
					SUM(rm.area)
					FROM rm
					LEFT OUTER JOIN rmcat ON rm.rm_cat = rmcat.rm_cat
					WHERE ${parameters['commonParameters']}
					AND ${parameters['emRestriction']}
					AND ${parameters['teamRmRestriction']} 
					AND rm.dwgname IS NOT NULL
					AND (rm.rm_cat IS NULL OR EXISTS (SELECT 1 FROM rmcat WHERE
					rm.rm_cat= rmcat.rm_cat AND rmcat.used_in_calcs !='no_totals'))
					AND rm.bl_id = wraped_rm.bl_id AND rm.fl_id = wraped_rm.fl_id AND rm.dwgname = wraped_rm.dwgname 
					GROUP BY rm.bl_id, rm.fl_id, rm.dwgname
				 ) ${sql.as} total_area,
					 
				  (	SELECT  SUM(rm.cap_em)  
					FROM rm  
					LEFT OUTER JOIN rmcat ON rm.rm_cat = rmcat.rm_cat
					WHERE ${parameters['commonParameters']}
					AND ${parameters['emRestriction']}
					AND ${parameters['teamRmRestriction']} 
					AND rm.dwgname IS NOT NULL 
					AND rm.bl_id = wraped_rm.bl_id AND rm.fl_id = wraped_rm.fl_id AND rm.dwgname = wraped_rm.dwgname 
					GROUP BY rm.bl_id, rm.fl_id, rm.dwgname 
				  ) ${sql.as} cap_em,				 
				  (    ( SELECT  SUM(rm.cap_em)  
							FROM rm  
							LEFT OUTER JOIN rmcat ON rm.rm_cat = rmcat.rm_cat
							WHERE rm.dwgname IS NOT NULL 
							AND rm.bl_id = wraped_rm.bl_id AND rm.fl_id = wraped_rm.fl_id AND rm.dwgname = wraped_rm.dwgname 
							AND not exists ( select 1 from rm_team where rm.bl_id = rm_team.bl_id AND rm.fl_id = rm_team.fl_id AND rm.rm_id = rm_team.rm_id and ( rm_team.date_end is null or ${sql.yearMonthDayOf('rm_team.date_end')} &gt;= ${parameters['selectDateStart']}) and ${sql.yearMonthDayOf('rm_team.date_start')} &lt;= ${parameters['selectDateEnd']} ) 
							GROUP BY rm.bl_id, rm.fl_id, rm.dwgname ) 
						- 
						( select count(1) from em where em.bl_id=wraped_rm.bl_id and em.fl_id=wraped_rm.fl_id and em.rm_id is not null) 
				  ) ${sql.as} total_team_cap,				 
				 wraped_rm.count_em,
				 wraped_rm.total_count, 
				 fl.area_gross_ext ${sql.as} area_gross_ext,
				 fl.area_gross_int ${sql.as} area_gross_int,
				 fl.area_rentable ${sql.as} area_rentable,
				 fl.area_usable ${sql.as} area_usable 
			  FROM 
					(	SELECT
							rm.bl_id ${sql.as} bl_id,
							rm.fl_id ${sql.as} fl_id,
							rm.dwgname ${sql.as} dwgname,
							COUNT(em.em_id) ${sql.as} count_em,
							COUNT(distinct rm.bl_id ${sql.concat} rm.fl_id ${sql.concat} rm.rm_id) ${sql.as} total_count
							FROM rm
							LEFT OUTER JOIN rmcat ON rm.rm_cat = rmcat.rm_cat
							LEFT OUTER JOIN em ON rm.rm_id = em.rm_id AND rm.fl_id = em.fl_id AND rm.bl_id = em.bl_id
							LEFT OUTER JOIN bl on rm.bl_id=bl.bl_id 
							WHERE ${sql.getVpaRestrictionForTable('bl')} and ${sql.vpaRestriction} 
							and ${parameters['commonParameters']} 
							AND ${parameters['emRestriction']} 
							AND ${parameters['teamRmRestriction']} 
							AND rm.dwgname IS NOT NULL 
							GROUP BY rm.bl_id, rm.fl_id, rm.dwgname
					)  ${sql.as}  wraped_rm 
					LEFT OUTER JOIN fl ON wraped_rm.fl_id = fl.fl_id and wraped_rm.bl_id = fl.bl_id  
		  ) ${sql.as} wraped_rm_for_oracleDb  
 		 LEFT OUTER JOIN  (
			select 
				wrap_rm_team.bl_id ${sql.as} blId , wrap_rm_team.fl_id ${sql.as} flId, wrap_rm_team.dwgname ${sql.as}  tDwgName,  
				CAST( ROUND( sum(
													CASE WHEN wrap_rm_team.teamCapcity IS NULL OR  wrap_rm_team.teamCapcity=0 THEN 0
													ELSE wrap_rm_team.teamOccupancy*1.0*wrap_rm_team.teamFloorCapcity/wrap_rm_team.teamCapcity
													END 
												), 0 )  AS int ) ${sql.as} floorTeamOccupancy 
			from ( 
				select 
					rt1.bl_id, rt1.fl_id, r1.dwgname,  rt1.team_id,  
					( select sum(team.pct_time/100.00) from team 
												where team.team_id=rt1.team_id 
												and (team.date_start is null or ${sql.yearMonthDayOf('team.date_start')} &lt;= ${parameters['asOfDate']})
												and (team.date_end is null or ${sql.yearMonthDayOf('team.date_end')} &gt;= ${parameters['asOfDate']})  
												and team.em_id is not null 
					) ${sql.as} teamOccupancy,
					sum(r1.cap_em) - sum (case when re.count_em is null then 0 else re.count_em end) ${sql.as} teamFloorCapcity, 
					avg(rt.team_cap) ${sql.as} teamCapcity 
				from rm_team ${sql.as} rt1
				left outer join rm ${sql.as} r1 on  rt1.bl_id=r1.bl_id and rt1.fl_id=r1.fl_id and rt1.rm_id=r1.rm_id 
				left outer join (
					select  em.bl_id, em.fl_id, em.rm_id, count( em.em_id) ${sql.as} count_em from em group by em.bl_id, em.fl_id, em.rm_id 					
				) ${sql.as} re on  rt1.bl_id=re.bl_id and rt1.fl_id=re.fl_id and rt1.rm_id=re.rm_id 
				left outer join (
					select rt.team_id, sum(r.cap_em)-sum(CASE WHEN re1.ec IS NULL THEN 0 ELSE re1.ec end) ${sql.as} team_cap 
					from rm_team ${sql.as} rt  
					left outer  join rm ${sql.as} r  on  rt.bl_id=r.bl_id and rt.fl_id=r.fl_id and rt.rm_id=r.rm_id  	
					left outer join ( select e1.bl_id, e1.fl_id, e1.rm_id, count( e1.em_id) ${sql.as} ec from em ${sql.as} e1 group by e1.bl_id, e1.fl_id, e1.rm_id ) ${sql.as} re1  on  rt.bl_id=re1.bl_id and rt.fl_id=re1.fl_id and rt.rm_id=re1.rm_id   
					where 	 (rt.date_start is null or ${sql.yearMonthDayOf('rt.date_start')} &lt;= ${parameters['asOfDate']})
								and (rt.date_end is null or ${sql.yearMonthDayOf('rt.date_end')} &gt;= ${parameters['asOfDate']}) 
					group by rt.team_id    
				) ${sql.as} rt on  rt1.team_id=rt.team_id  

				where  (rt1.date_start is null or ${sql.yearMonthDayOf('rt1.date_start')} &lt;= ${parameters['asOfDate']})
							and (rt1.date_end is null or ${sql.yearMonthDayOf('rt1.date_end')} &gt;= ${parameters['asOfDate']}) 
				group by rt1.bl_id, rt1.fl_id, r1.dwgname,  rt1.team_id 
			) ${sql.as} wrap_rm_team 
			group by wrap_rm_team.bl_id, wrap_rm_team.fl_id, wrap_rm_team.dwgname 
		 ) ${sql.as} floor_team ON floor_team.blId=wraped_rm_for_oracleDb.bl_id and wraped_rm_for_oracleDb.fl_id = floor_team.flId and wraped_rm_for_oracleDb.dwgname = floor_team.tDwgName  
		  
		  WHERE ${parameters['totalArea']} and ${parameters['totalTeamCap']} 
        </sql>

		<table name="rm" role="main"/>
        <table name="em" role="standard"/>
        <table name="bl" role="standard"/>

        <field table="rm" name="bl_id" dataType="text"/>
        <field table="rm" name="fl_id" dataType="text"/>
        <field table="rm" name="dwgname" dataType="text"/>
        <field table="rm" name="total_area" dataType="number" size="8" decimals="2" showTotals="true" numericFormat="area"/>
        <field table="rm" name="cap_em" dataType="integer" showTotals="true">
            <sql dialect="generic">cap_em</sql>
        </field>
        <field table="rm" name="count_em" dataType="integer" showTotals="true">
            <sql dialect="generic">count_em</sql>
        </field>
        <field table="rm" name="total_count" dataType="number" decimals="0" showTotals="true"/>
        <field table="rm" name="total_team_cap" dataType="number" decimals="0" showTotals="true"/>
        <field table="rm" name="area_gross_ext" dataType="number" decimals="2" showTotals="true" numericFormat="area"/>
        <field table="rm" name="area_gross_int" dataType="number" decimals="2" showTotals="true" numericFormat="area"/>
        <field table="rm" name="area_rentable" dataType="number" decimals="2" showTotals="true" numericFormat="area"/>
        <field table="rm" name="area_usable" dataType="number" decimals="2" showTotals="true" numericFormat="area"/>
        <field table="rm" name="floorTeamOccupancy" dataType="integer"/>
        
        <sortField table="rm" name="bl_id"/>
