    <message name="licenseLevel">License Level</message>
    <message name="totalUsers">Total Users</message>
    <message name="namedUsers">Total Named Users</message>
    <message name="concurrentUsers">Total Concurrent Users</message>
    <message name="inUse">Licenses in Use on this Server</message>
    <message name="available">Licenses Available on this Server</message>
    <message name="totalPerServer">Total Licenses on this Server</message>
    <message name="numberOfCores">Core Licenses</message>
    <message name="numberOfEALs">Enterprise Access Licenses (EALs)</message>
    <message name="otherOptions">Other Options</message>

    <message name="domains">Application Domains Licensed</message>
