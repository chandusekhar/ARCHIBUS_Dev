<view version="2.0">
    <title>About ARCHIBUS Web Central </title>

    <css>
        .title { font-size: 16px; }
    </css>

    <dataSource id="ds_domains">
       	<table name="afm_products" role="main"/>
       	<field table="afm_products" name="product_id"/>
		<field table="afm_products" name="title"/>
    </dataSource>

    <dataSource id="ds_activities">
       	<table name="afm_activities" role="main"/>
       	<field table="afm_activities" name="activity_id"/>
		<field table="afm_activities" name="title"/>
    </dataSource>
            
    <panel type="html" id="customerInfoPanel">
        <title>Customer Information</title>
        <action id="refresh">
            <title>Refresh License Information</title>
        </action>
        <action id="print">
            <title>Print License Information</title>
        </action>
        <html>
            <table id="customerInfoTable" cellpadding="0" cellspacing="0" border="0" class="panel htmlWizardPanel">
            </table>
        </html>
    </panel>    

    <panel type="html" id="concurrentUsersPanel">
        <title>Program Licenses Levels - Concurrent Users</title>
        <html>
            <table id="concurrentUsersTable" cellpadding="0" cellspacing="0" border="0" class="panel htmlWizardPanel">
            </table>
        </html>
    </panel>    

    <panel type="html" id="namedUsersPanel">
        <title>Program Licenses Levels - Named Users</title>
        <html>
            <table id="namedUsersTable" cellpadding="0" cellspacing="0" border="0" class="panel htmlWizardPanel">
            </table>
        </html>
    </panel>    

    <panel type="html" id="otherOptionsPanel">
        <html>
            <table id="otherOptionsTable" cellpadding="0" cellspacing="0" border="0" class="panel htmlWizardPanel">
            </table>
        </html>
    </panel>    

    <panel type="html" id="domainsPanel">
        <html>
            <table id="domainsTable" cellpadding="0" cellspacing="0" border="0" class="panel htmlWizardPanel">
            </table>
        </html>
    </panel>    

    <panel type="html" id="activitiesPanel">
        <html>
            <table id="activitiesTable" cellpadding="0" cellspacing="0" border="0" class="panel htmlWizardPanel">
            </table>
        </html>
    </panel>    

    <message name="licenseViewTitle">License Information</message>

