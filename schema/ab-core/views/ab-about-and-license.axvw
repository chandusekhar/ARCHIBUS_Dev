    <message name="licensedVersion">Licensed Version</message>
    <message name="licensedTo">Licensed To</message>
    <message name="serialNumber">Serial Number</message>
    <message name="buildNumber">Build Number</message>
    <message name="expirationDate">Expiration Date</message>
