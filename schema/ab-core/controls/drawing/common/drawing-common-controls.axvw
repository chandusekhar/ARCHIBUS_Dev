<!-- reusable .axvw file for the Drawing control that takes care of including all control resources: JS, CSS, localized strings in AXVW, libraries, etc. -->
 
<view version="2.0">
   
    <!-- WDR DrawingSvgService -->
    <js url="/archibus/dwr/interface/DrawingSvgService.js" />
    
    <!-- d3 SVG library -->
    <js file="d3.min.js"/>
    <js file="d3.tip.v0.6.3.js"/>

    <!-- common utlities -->
    <js file="drawing-namespace.js"/>
        
    <!--  drawng control -->
    <js file="drawing-base.js"/>
    <js file="drawing-control.js"/>

    <!-- Controllers -->
    <js file="drawing-controller.js"/>
    <js file="addons-controller.js"/>
    <js file="panzoom-controller.js"/>
    <js file="asset-controller.js"/>
    <js file="select-controller.js"/>
    <js file="highlight-controller.js"/>
    <js file="event-controller.js"/>

    <!-- UI Component -->
    <js file="layers-popup.js"/>
    <js file="asset-locator.js"/>
    <js file="info-bar.js"/>
    <js file="info-window.js"/>
    <js file="navigation-toolbar.js"/>
    <js file="datasource-selector.js"/>
    <js file="asset-tooltip.js"/>
    <js file="select-window.js"/>
    
    <!-- style -->
    <css file="htmldrawing.css"/>

</view>