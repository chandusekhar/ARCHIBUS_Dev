<view version="2.0">
	<title translatable="true">Package and Deploy Wizard</title>
	<js file="ab-application-update-wizard.js"/>

	
	<!-- Package Deployment tab -->

	<tabs workflow="free" id="appUpdWizTabs" tabRefreshPolicy="refreshOnLoad" cssClass="wizard">	
		<event type="afterTabChange">
		</event>

		<tab name="wizardTabs_0" file="ab-application-update-verify-backup.axvw" useFrame="true" selected="true">
			<title translatable="true">Verify Backup</title>
		</tab>
		<tab name="wizardTabs_1" file="ab-application-update-deploy-type.axvw" useFrame="true">
			<title translatable="true">Specify Deployment Type</title>
		</tab>
		<tab name="wizardTabs_2" file="ab-application-update-verify-archive.axvw" useFrame="true">
			<title translatable="true">Verify Archive</title>
		</tab>
		<tab name="wizardTabs_3" file="ab-application-update-update-type.axvw" useFrame="true">
			<title translatable="true">Specify Update Type</title>
		</tab>
		<tab name="wizardTabs_4" file="ab-application-update-preserve-data.axvw" useFrame="true">
			<title translatable="true">Preserve Data</title>
		</tab>
		<tab name="wizardTabs_5" file="ab-application-update-preserve-extensions.axvw" useFrame="true">
			<title translatable="true">Preserve Extensions(optional)</title>
		</tab>
		<tab name="wizardTabs_6" file="ab-application-update-package-deploy.axvw" useFrame="true">
			<title translatable="true">Package Deployment</title>
		</tab>
		<tab name="wizardTabs_7" file="ab-application-update-review.axvw" useFrame="true">
			<title translatable="true">Review</title>
		</tab>
	</tabs>
</view>