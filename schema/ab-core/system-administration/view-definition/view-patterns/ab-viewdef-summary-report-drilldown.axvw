            <command type="exportPanel" outputType="xls" panelId="panel_abViewdefSummaryReportDrilldown_popup"/>
        </action>
        <!--ViewDef-DataTgrp-Action-Properties-XLS-End-->
        <!--ViewDef-DataTgrp-Action-Properties-TXFR-Begin-->
        <action id="abViewdefSummaryReportDrilldown_exportTXFR">
