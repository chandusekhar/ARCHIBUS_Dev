        <action id="abViewdefSummaryReportDrilldown_refresh">
            <title translatable="true">Refresh</title>
            <command type="showPanel" panelId="abViewdefSummaryReportDrilldown_detailsPanel"/>
        </action>
        
        <!--ViewDef-Chart-Popup-Event-Begin-->
        <event type="onClickItem">
            <!--ViewDef-Chart-Popup-Event-GroupByDate-CallFunction-Begin-->
            <!--ViewDef-Chart-Popup-Event-GroupByDate-CallFunction-End-->
            <!--ViewDef-Chart-Popup-Event-OpenDialog-Begin-->
            <command type="openDialog" panelId="panel_abViewdefSummaryReportDrilldown_popup" width="600" height="400"/>
            <!--ViewDef-Chart-Popup-Event-OpenDialog-End-->
        </event>
        <!--ViewDef-Chart-Popup-Event-End-->
    </panel>
    
    <!--ViewDef-Chart-Popup-Begin-->
    <dataSource id="abViewdefSummaryReportDrilldown_ds_popupDetails">  
